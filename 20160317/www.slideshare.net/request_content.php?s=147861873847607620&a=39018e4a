				target: {x:0,y:0},
				width:21,
				height:27,
				borderpadding:10,
				jumpcounter:0
			};
			
			if ( Math.random() < 0.4 ) distractor.active = true;

			var adConfig = {
				a:200,
				showtime:8,
				itemDifftime:0.15,
				iscrollRoundtrip:8,
				smallImgWidth:120,
				bigImgWidth:234,
				width:300,
				height:250,
				animStart:true,
				animRunning:true
			};
			
			var screenpositions = [
				{x:31,y:2},
				{x:31,y:84},
				{x:31,y:166}
			];
			
			if ( nonBranded ) {
				adConfig.animStart = false;
				adConfig.animRunning = false;
				distractor.active = false;
				screenpositions[0].y = 1;
				screenpositions[2].y = 167;
				
					var eDS = document.getElementById('b[0]').style;
					eDS.background = 'transparent';
					eDS.backgroundColor = '#b0b0b3';
					eDS.border= '1px dotted #888890';
				
					var eDS = document.getElementById('b[1]').style;
					eDS.background = 'transparent';
					eDS.backgroundColor = '#b0b0b3';
					eDS.border= '1px dotted #888890';
				
					var eDS = document.getElementById('b[2]').style;
					eDS.background = 'transparent';
					eDS.backgroundColor = '#b0b0b3';
					eDS.border= '1px dotted #888890';
				
			}

			init();
		
		</script>
	

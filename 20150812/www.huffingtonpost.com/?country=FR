	jQuery('#jcarousel-next-horizontal_14392801240e47a1bd1').live('click', function(){
		var slide = parseInt(jQuery('#slide_14392801240e47a1bd1').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14392801240e47a1bd1').html(slide);
		if(jQuery('#carousel_14392801240e47a1bd1').outerHeight(true) < jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14392801240e47a1bd1').animate({
				height: jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14392801240e47a1bd1').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14392801240e47a1bd1').animate({
			height: jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14392801240e47a1bd1').live('click', function(){
		var slide = parseInt(jQuery('#slide_14392801240e47a1bd1').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14392801240e47a1bd1').html(slide);
		if(jQuery('#carousel_14392801240e47a1bd1').outerHeight(true) < jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14392801240e47a1bd1').animate({
				height: jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14392801240e47a1bd1').children('li:last').detach();
		jQuery('#carousel_14392801240e47a1bd1').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14392801240e47a1bd1').animate({
			height: jQuery('#carousel_14392801240e47a1bd1 .jcarousel-item-' + slide).outerHeight(true)
		});
	});

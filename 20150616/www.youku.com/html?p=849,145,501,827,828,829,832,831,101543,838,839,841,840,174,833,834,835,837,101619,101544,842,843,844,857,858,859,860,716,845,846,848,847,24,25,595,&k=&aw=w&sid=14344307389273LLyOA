//-----_YoukuAd-----
var _YoukuAd = {};
_YoukuAd.uaMatch = function(ua){
	ua = ua.toLowerCase();
	//-----Mobile-----
	var ismobile = false;
	var mobilematch = /(iPhone|iPad|iPod|Android|iTouch|Windows Phone|Symbian|BlackBerry|Nokia|TouchPad)/im.exec( ua );
	if(	mobilematch != null && mobilematch[1] != null){
		ismobile = true;
	}
	//-----Browser-----
	var browsermatch = /(MSIE|Trident|IE)/im.exec( ua ) ||														//Ie
					  /^(?=.*(Chrome|CrMo|CriOS))(?!.*OPR\/|Web Preview).+$/im.exec( ua )||			//Chrome
					  /^(?=.*(Safari))(?!.*OPR\/|Web Preview|Googlebot-Mobile).+$/im.exec( ua )||		//Safari
					  /(OPR\/|OPERA)/im.exec( ua )||																			//Opera
					   /^(?=.*(Firefox))(?!.*ggpht.com).+$/im.exec( ua );													//Firefox
	var browser = "unknown";
	if(	browsermatch != null && browsermatch[1] != null){
		if('opr/' == browsermatch[ 1 ]){
			browsermatch[1] = browsermatch[1].substring(0, browsermatch[1].indexOf("\/"));
		}
		browser = _BrowserMap[browsermatch[ 1 ]].name;
	}else{
		browser= _BrowserMap["unknown"].name;
	}
	//-----Version-----
	var version = "0";
	if('firefox' ==browser){//Firefox
		var firefoxversionmatch = /Firefox[\/]([\w.]+)/im.exec( ua );
		if(	firefoxversionmatch != null && firefoxversionmatch[1] != null){
			version = firefoxversionmatch[1];
		}
	}else if('ie' ==browser){//Ie
		var ieversionmatch = /^(?=.*(MSIE) ([\w.]+))(?!.*Trident\/7.0).+$/im.exec( ua ) || /(Trident)[\/]([\w.]+)/im.exec( ua ) || /(IE) ([\w.]+)/im.exec( ua );
		if(	ieversionmatch != null && ieversionmatch[1] != null && ieversionmatch[2] != null){
			if('msie' == ieversionmatch[ 1 ]){
				version = ieversionmatch[2];
			}else if('ie' == ieversionmatch[ 1 ]){
				version = ieversionmatch[2];
			}else if('trident' == ieversionmatch[ 1 ]){
				version = "11.0";
			}
		}
	}else if('chrome' ==browser){//Chrome
		var chromeversionmatch = /Chrome[\/]([\w.]+)/im.exec( ua ) || /(?:CrMo|CriOS|Chrome)[\/]([\w.]+)/im.exec( ua );
		if(	chromeversionmatch != null && chromeversionmatch[1] != null){
			version = chromeversionmatch[1];
		}
	}else if('opera' ==browser){//Opera
		var operaversionmatch = /OPR[\/]([\w.]+)/im.exec( ua ) || /Opera[\/]([\w.]+)/im.exec( ua );
		if(	operaversionmatch != null && operaversionmatch[1] != null){
			version = operaversionmatch[1];
		}
	}else if('safari' ==browser){//Safari
		var safariversionmatch = /Version[\/]([\w.]+)/im.exec( ua );
		if(	safariversionmatch != null && safariversionmatch[1] != null){
			version = safariversionmatch[1];
		}
	}
	//-----Return-----
	return {
		ismobile: ismobile || false,
		browser: browser || "unknown",
		version: version || "0",
		ua : ua
	};
}
//-----Init-----
function setAppleAdTop(){

	//-----Browser Logic Start-----
	//å¤æ­æµè§å¨çæ¬
	var Browser = _YoukuAd.uaMatch( navigator.userAgent );
	//ç§»å¨ç«¯ä¸åºå¹¿å
	if(Browser.ismobile){
		return;
	}
	//æµè¯surfaceIEä¸åºå¹¿å
	//if (Browser.browser == "ie" && navigator.userAgent.toUpperCase().indexOf('ARM') > 0) {
	//	return;
	//}

	if ( (Browser.browser == "ie" && navigator.userAgent.toUpperCase().indexOf('ARM') > 0) || (Browser.browser == "ie" && navigator.userAgent.toLowerCase().indexOf('tablet pc') > 0 ) ) {
		return;
	   }

	//å¤æ­å®½çªå±å¹
	var _ad_width = parseInt((/\bw(\d+)\b/.exec(document.getElementsByTagName('body')[0].className)||[])[1]||970);
	if(_ad_width && _ad_width != null && typeof(_ad_width) != "undefined"){
		if( _ad_width < 940 ){
			return;
		}
	}
	//è½¬æ¢æ°å¼
	var floatVersionValue = parseFloat(Browser.version);
	//-----Browser Logic End-----

	//-----Ad Logic Start-----
	//-----Ie-----
	if(Browser.browser == "ie"){	//IE
		//-----Ie Windows-----
		if( Browser.ua.indexOf("windows") != -1 ){	//PC
			//IE9+
			if(floatVersionValue >= 9 ){
				//å±ç¤ºH5å¹¿å
				_showH5();
			//IEå¶å®
			}else{
				//å±ç¤ºJpgå¹¿å
				_showJpg("ie");
				_showCount('ie');
			}
		//-----Ie Mac-----
		}else{	//Macintosh
			_showJpg("defaultt");
			_showCount('defaultt');
		}
	//-----Firefox-----
	}else if(Browser.browser == "firefox"){	//Firefox
		//FireFox 35+
		if(floatVersionValue >= 35){
		//å±ç¤ºH5å¹¿å
		_showH5();
		//Firefoxå¶å®
		}else{
			_showJpg("firefox");
			_showCount('firefox');
		}
	//-----Safari-----
	}else if(Browser.browser == "safari"){	//Safari
		//-----Safari Windows-----
		if( Browser.ua.indexOf("windows") != -1 ){	//PC
			_showJpg("safari");_showCount('safari');
		//-----Safari Mac-----
		}else{	//Macintosh
			//Safari 7+
			if(floatVersionValue >= 7){
				//å±ç¤ºH5å¹¿å
				_showH5();
			}else{
				//å±ç¤ºJpgå¹¿å
				_showJpg("safari");
				_showCount('safari');
			}
		}
	//-----Chrome-----
	}else if(Browser.browser == "chrome"){	//Chrome
		//Chrome 36+
		if(floatVersionValue >= 36){
			//å±ç¤ºH5å¹¿å
			_showH5();
		//Chromeå¶å®
		}else{
			//å±ç¤ºJpgå¹¿å
			_showJpg("chrome");
			_showCount('chrome');
		}
	//-----Opera-----
	}else if(Browser.browser == "opera"){	//Opera
		//å±ç¤ºJpgå¹¿å
		_showJpg("opera");
		_showCount('opera');
	//-----Default-----
	}else{	//Default
		//å±ç¤ºJpgå¹¿å
		_showJpg("defaultt");
		_showCount('defaultt');
	}
	//-----Ad Logic End-----
}
function _showH5(){
	//åå»ºH5
	var ad = document.getElementById("youku_ad_apple_node");
	if(ad && window.innerWidth >= 940){

		//TODO ä¿®æ¹å°åï¼çæµ
		ad.innerHTML = '<iframe scrolling="no" id="youku_ad_apple_node_iframe" style="width: 970px; height: 350px; border: 0pt none;" height="350" width="970" frameborder="0" marginheight="0" marginwidth="0" border="0" src="http://applead.youku.com/0612/youku/youku_cn_index.html"></iframe>';
		ad.style.display = 'block';
		document.getElementById('ab_145').parentNode.parentNode.parentNode.style.height="375px";
		//è®°å½æåçæµ
		var img_atm = document.createElement("img");
		img_atm.width=0;
		img_atm.height=0;
		img_atm.src="http://s.cr-nielsen.com/hat?_t=i&_htsinfo=SSYyJjgwMDAwMDI1JjEwMDA2MDMxJjMwMTA1ODY5Jt3f";
		document.getElementsByTagName('HEAD').item(0).appendChild(img_atm);

	}
}
function _showJpg(type){
	//åå»ºJpg
	var ad = document.getElementById("youku_ad_apple_node");
	if(ad && window.innerWidth >= 940){

		var click_href = "http://www.youku.com";
		if( _ClickMap[type] ){
			click_href = _ClickMap[type].url;
		}
		ad.innerHTML = '<a target="_blank"  style="outline: none;" onclick="clickCount(\'' + type + '\');" href="' + click_href + '"><img border="0" width="970px" height="350px" src="http://applead.youku.com/0612/youku/static_endcard_you_970-cn.jpg"></a>';
		ad.style.display = 'block';
		document.getElementById('ab_145').parentNode.parentNode.parentNode.style.height="375px";
		
	}
}


function _showCount(type){
	if( _ShowImpMap[type] ){
		//è®°å½æåçæµ
		var img_atm = document.createElement("img");
		img_atm.width = 0;
		img_atm.height = 0;
		img_atm.src = _ShowImpMap[type].url;
		document.getElementsByTagName('HEAD').item(0).appendChild(img_atm);
		//è®°å½å®¢æ·æåçæµ
		var img_client_atm = document.createElement("img");
		img_client_atm.width = 0;
		img_client_atm.height = 0;
		img_client_atm.src = _ShowImpMap[type].client_url + (new Date()).valueOf();
		document.getElementsByTagName('HEAD').item(0).appendChild(img_client_atm);
	}
}
function clickCount(type){
	if( _ClickImpMap[type] ){
		//è®°å½ç¹å»çæµ
		var img_atm = document.createElement("img");
		img_atm.width = 0;
		img_atm.height = 0;
		img_atm.src = _ClickImpMap[type].url;
		document.getElementsByTagName('HEAD').item(0).appendChild(img_atm);
	}
}

                            else if(document.getElementsByTagName("head").length>0)
                            {
                                document.getElementsByTagName("head")[0].appendChild(unblock_script);
                            }
                            else if(document.getElementsByTagName("body").length>0)
                            {
                                document.getElementsByTagName("body")[0].appendChild(unblock_script);
                            }
                        }

                        DM_ResourcePlanner.add(
                            "ad-unblock-plugin",
                            function() {
                                unblockWrapper();
                            }
                        );
                    </script>


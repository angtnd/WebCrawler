<script src="js/cycle.all.js" type="text/javascript"></script>
<script src="js/jquery.easing.1.3.js" type="text/javascript"></script>
<script type="text/javascript">

$(document).ready(function(){
	$('#myslides').cycle({
		fx: 'fade',  
        easing: 'easeInExpo',
		speed:	250,
		timeout:2000,
		pause: 1,
		after:   onAfter
	});

	$('#myslides a').hover(function(){
		$(this).children('#play').animate({opacity: '0.25'}, 200).animate({opacity: '1'}, 200);
	},
	function(){
		$(this).children('#play').css({opacity: '1'});
	});

});

function onAfter(curr, next, opts) {
	$('#ss', next).css({left: '0px'}).animate({left: '-150px'}, 2300);
}
</script>

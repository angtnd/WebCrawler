	jQuery('#jcarousel-next-horizontal_14425314599570194712').live('click', function(){
		var slide = parseInt(jQuery('#slide_14425314599570194712').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14425314599570194712').html(slide);
		if(jQuery('#carousel_14425314599570194712').outerHeight(true) < jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14425314599570194712').animate({
				height: jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14425314599570194712').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14425314599570194712').animate({
			height: jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14425314599570194712').live('click', function(){
		var slide = parseInt(jQuery('#slide_14425314599570194712').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14425314599570194712').html(slide);
		if(jQuery('#carousel_14425314599570194712').outerHeight(true) < jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14425314599570194712').animate({
				height: jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14425314599570194712').children('li:last').detach();
		jQuery('#carousel_14425314599570194712').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14425314599570194712').animate({
			height: jQuery('#carousel_14425314599570194712 .jcarousel-item-' + slide).outerHeight(true)
		});
	});

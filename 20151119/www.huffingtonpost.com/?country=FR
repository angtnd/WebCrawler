		var slide = parseInt(jQuery('#slide_14479506173642cc5743').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14479506173642cc5743').html(slide);
		if(jQuery('#carousel_14479506173642cc5743').outerHeight(true) < jQuery('#carousel_14479506173642cc5743 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14479506173642cc5743').animate({
				height: jQuery('#carousel_14479506173642cc5743 .jcarousel-item-' + slide).outerHeight(true)
			});

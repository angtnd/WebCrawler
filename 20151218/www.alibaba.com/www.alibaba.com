function shouldShowBanner() {
    var filterList = ['offer.alibaba.com'];

    var href = window.location.href;

    for (var i = 0, len = filterList.length; i < len; i++) {
        if (href.indexOf(filterList[i]) !== -1) {
            return false;
        }
    }

    if(SCHeader.J('#J-nav-menu').length > 0){
        return false;
    }

    return true;
}

function showTopReached() {

    var banners = [{
        bannerUrl: 'https://img.alicdn.com/tps/TB12bsiKVXXXXX2aXXXXXXXXXXX-990-70.png',
        href: 'http://activities.alibaba.com/alibaba/instant_quotes.php?tracelog=1216_dingtong',
        color: '#CE0A54',
        time: '2015-12-15~2015-12-20'
    }];

    var container = SCHeader.J('.mm-sc-new-header-v5').length === 0 ? SCHeader.J('.ui-header-dynamic') : SCHeader.J('.mm-sc-new-header-v5');

    if(container.length === 0){
        container = SCHeader.J('body');
    }

    new SCHeader.TopReached({
        type: 'small',
        urlList: banners,
        appendType: 'prepend',
        parentNode: container
    });
}

SCHeader.J(function(){
    if(shouldShowBanner()) {

        showTopReached();
    }
});
		
		

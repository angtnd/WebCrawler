		var slide = parseInt(jQuery('#slide_145045981930f1b35f38').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_145045981930f1b35f38').html(slide);
		if(jQuery('#carousel_145045981930f1b35f38').outerHeight(true) < jQuery('#carousel_145045981930f1b35f38 .jcarousel-item-' + slide).outerHeight(true))

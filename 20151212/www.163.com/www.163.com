            wrap.$('.slider-ctrl-prev').addEvent('click, mouseover', function(ev){ ev.preventDefault(); stopRepeat(); });
            wrap.$('.slider-ctrl-next').addEvent('click, mouseover', function(ev){ ev.preventDefault(); stopRepeat(); });
            isInit = true;
        }

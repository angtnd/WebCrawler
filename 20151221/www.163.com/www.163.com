				for(var i=0,j=data.length;i<j;i++){
					ret.push(replaceAction(data[i]));
				}
				return ret.join("");
				function replaceAction(object){
					return temp.replace(regexp || (/\\?\{([^}]+)\}/g), function(match, name){

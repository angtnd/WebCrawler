	jQuery('#jcarousel-next-horizontal_1447340237c40599d145').live('click', function(){
		var slide = parseInt(jQuery('#slide_1447340237c40599d145').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1447340237c40599d145').html(slide);
		if(jQuery('#carousel_1447340237c40599d145').outerHeight(true) < jQuery('#carousel_1447340237c40599d145 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1447340237c40599d145').animate({
				height: jQuery('#carousel_1447340237c40599d145 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1447340237c40599d145').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1447340237c40599d145').animate({
			height: jQuery('#carousel_1447340237c40599d145 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1447340237c40599d145').live('click', function(){
		var slide = parseInt(jQuery('#slide_1447340237c40599d145').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1447340237c40599d145').html(slide);
		if(jQuery('#carousel_1447340237c40599d145').outerHeight(true) < jQuery('#carousel_1447340237c40599d145 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1447340237c40599d145').animate({
				height: jQuery('#carousel_1447340237c40599d145 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1447340237c40599d145').children('li:last').detach();
		jQuery('#carousel_1447340237c40599d145').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

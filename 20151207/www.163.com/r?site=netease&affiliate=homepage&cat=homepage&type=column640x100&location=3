document.write(' ID=flashad WIDTH=640 HEIGHT=100>');
document.write(' <PARAM NAME=movie VALUE="http://img1.126.net/channel4/021242/jh640100_1204a.swf"> '); 
document.write(' <PARAM NAME=quality VALUE=autohigh> ');
document.write(' <PARAM NAME=wmode VALUE=opaque> ');

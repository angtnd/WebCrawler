			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_144627895486ba51a9100').animate({
			height: jQuery('#carousel_144627895486ba51a9100 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_144627895486ba51a9100').live('click', function(){
		var slide = parseInt(jQuery('#slide_144627895486ba51a9100').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_144627895486ba51a9100').html(slide);
		if(jQuery('#carousel_144627895486ba51a9100').outerHeight(true) < jQuery('#carousel_144627895486ba51a9100 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_144627895486ba51a9100').animate({
				height: jQuery('#carousel_144627895486ba51a9100 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_144627895486ba51a9100').children('li:last').detach();

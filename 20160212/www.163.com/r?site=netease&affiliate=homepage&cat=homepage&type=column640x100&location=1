document.write(' <PARAM NAME=movie VALUE="http://img1.126.net/channel8734/021096/640100_1112.swf"> '); 
document.write(' <PARAM NAME=quality VALUE=autohigh> ');
document.write(' <PARAM NAME=wmode VALUE=opaque> ');
document.write(' <EMBED SRC="http://img1.126.net/channel8734/021096/640100_1112.swf" QUALITY=autohigh  wmode=opaque'); 

<script>
	var autoUpdateUtil = function() {
		return {
			getCookie : function(name) {
				var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
				if (arr != null) return unescape(arr[2]);
				return null;
			},
			getUrlParam : function(name) {
				var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
				var r = window.location.search.substr(1).match(reg);
				if (r!=null) return r[2];
				return "";
			}
		}
	} ();

	jQuery(function() {
		try {
			var autoUpdateParam = autoUpdateUtil.getUrlParam("autoUpdate");
			if (window.location.host == 'www.sohu.com' && 'false' != autoUpdateParam) {
				var autoUpdateFlag = false;
				var userType = 1;
				if ('true' == autoUpdateParam) {
					autoUpdateFlag = true;

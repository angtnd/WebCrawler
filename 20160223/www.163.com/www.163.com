    var lazyRenderFragment = new LazyRenderFragment({
        elements: 'textarea.lzrd',
        VMMargin: 100
    },{
        elements: 'img.lzrd',
        sourceAttribute: 'data-src',
        VMMargin: 50
    }, {
        elements: 'textarea.lzrd'
    });
    lazyRenderResource.start(true);
    lazyRenderFragment.start(true);
    var preLoadFragment = function(selector, ccount, rate){
        selector = selector || 'textarea.lzrd';
        ccount = ccount || 2;
        rate = rate || 1000;
        var task = new TaskQueue(ccount, rate, true);
        $(selector).each(function(){
            task.add({
                context: this,
                fn: function(){
                    lazyRenderFragment.render(this);
                }
            })
        });
        task.start();
    };
    var startFragmentPreload = function(){
        if(!window.LRC_PRELOAD_FRAGMENT_SELECTOR || startFragmentPreload.hasRun){
            return false;
        }
        startFragmentPreload.hasRun = true;
        preLoadFragment(window.LRC_PRELOAD_FRAGMENT_SELECTOR, window.LRC_PRELOAD_FRAGMENT_CCOUNT, window.LRC_PRELOAD_FRAGMENT_RATE);
    };
    $(function(){
        $(window).load(startFragmentPreload);
        //max wait time
        setTimeout(startFragmentPreload, window.LRC_WAIT_LOAD_TIMEOUT == undefined ? 4500 : window.LRC_WAIT_LOAD_TIMEOUT);
    });
});

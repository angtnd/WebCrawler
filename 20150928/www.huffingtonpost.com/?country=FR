		var slide = parseInt(jQuery('#slide_14433654570fa8a78a99').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14433654570fa8a78a99').html(slide);
		if(jQuery('#carousel_14433654570fa8a78a99').outerHeight(true) < jQuery('#carousel_14433654570fa8a78a99 .jcarousel-item-' + slide).outerHeight(true))

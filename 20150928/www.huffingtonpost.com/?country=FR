				height: jQuery('#carousel_14433654570fa8a78a99 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14433654570fa8a78a99').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14433654570fa8a78a99').animate({
			height: jQuery('#carousel_14433654570fa8a78a99 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14433654570fa8a78a99').live('click', function(){
		var slide = parseInt(jQuery('#slide_14433654570fa8a78a99').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14433654570fa8a78a99').html(slide);
		if(jQuery('#carousel_14433654570fa8a78a99').outerHeight(true) < jQuery('#carousel_14433654570fa8a78a99 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14433654570fa8a78a99').animate({
				height: jQuery('#carousel_14433654570fa8a78a99 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14433654570fa8a78a99').children('li:last').detach();
		jQuery('#carousel_14433654570fa8a78a99').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

var pix=pix||{};pix.ad_depend_id=[];pix.ad_depend_count=[];pix.ad_depend_group=[];pix.ad_extra_data=[];pix.ad_field_embed=[];pix.ad_embed_field=[];pix.ad_autoplay=[];pix.ad_noautoplay=[];pix.ad_loaded=function(d,c,a,b){pix.ad_field_embed[""+d]=c;pix.ad_embed_field[""+c]=d;pix.ad_autoplay[""+c]=b;if(a&&c!=("PT"+a)){pix.ad_depend_count["PT"+a]++}if(a){if(!pix.ad_depend_group["PT"+a]){pix.ad_depend_group["PT"+a]=[]}pix.ad_depend_id[c]="PT"+a;pix.ad_depend_group["PT"+a].push(c)}var e=setInterval(function(){var h=jQuery("#"+c),f,g=(pix.ad_embed_field[c]);if(h.length>0&&"undefined"!==typeof(h.get(0).PercentLoaded)&&h.get(0).PercentLoaded()>=100){if(!!(f=pix.ad_depend_id[c])){pix.ad_depend_count[f]--;if(pix.ad_depend_count[f]>0){return}g=pix.ad_embed_field[f]}jQuery("#"+c).addClass("swf_ready");pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)");clearInterval(e);jQuery(window).scroll()}},50)};pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)");jQuery(function(){var a=function(){if(!pix.ad_embeds.length){return}pix.ad_embeds.each(function(c){var g=pix.ad_embeds.eq(c),f=g.attr("id"),b,e,d=jQuery("#"+f);if(g.offset()&&(g.offset().top+g.height()>jQuery(window).scrollTop())&&(g.offset().top<jQuery(window).scrollTop()+jQuery(window).height())){e=(pix.ad_embed_field[f]);if(!!(b=pix.ad_depend_id[f])){pix.ad_depend_count[b]--;if(pix.ad_depend_count[b]>0){return}e=pix.ad_embed_field[b]}if(pix.ad_load_init[e]){pix.ad_load_init[e](f)}if(pix.ad_autoplay[f]){if(!pix.get_cookie(f+"_played")&&!pix.ad_noautoplay[e]){pix.ad_load(e);pix.set_cookie(f+"_played",true)}else{if(d.length>0){d.get(0).TGotoLabel("/","close")}}}g.addClass("swf_loaded");pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)")}})};jQuery(window).scroll(a);jQuery(window).resize(a)});pix.get_cookie=function(c){var d=document.cookie.split(";"),b,a;for(b=0;b<d.length;b++){a=d[b].split("=");if(a[0].replace(/^\s+|\s+$/g,"")==c){return a[1]}}return};pix.set_cookie=function(a,b){var f=new Date(),e,c;if(null===b){e=f.getMonth()-1;c=
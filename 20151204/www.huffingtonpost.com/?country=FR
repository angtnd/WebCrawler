		jQuery('#carousel_1449249978af9bc02864').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1449249978af9bc02864').animate({
			height: jQuery('#carousel_1449249978af9bc02864 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1449249978af9bc02864').live('click', function(){
		var slide = parseInt(jQuery('#slide_1449249978af9bc02864').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1449249978af9bc02864').html(slide);
		if(jQuery('#carousel_1449249978af9bc02864').outerHeight(true) < jQuery('#carousel_1449249978af9bc02864 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1449249978af9bc02864').animate({
				height: jQuery('#carousel_1449249978af9bc02864 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1449249978af9bc02864').children('li:last').detach();
		jQuery('#carousel_1449249978af9bc02864').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1449249978af9bc02864').animate({
			height: jQuery('#carousel_1449249978af9bc02864 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

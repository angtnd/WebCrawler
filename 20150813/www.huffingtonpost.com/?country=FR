	jQuery('#jcarousel-next-horizontal_14393686160e47a1bd16').live('click', function(){
		var slide = parseInt(jQuery('#slide_14393686160e47a1bd16').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14393686160e47a1bd16').html(slide);
		if(jQuery('#carousel_14393686160e47a1bd16').outerHeight(true) < jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14393686160e47a1bd16').animate({
				height: jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14393686160e47a1bd16').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14393686160e47a1bd16').animate({
			height: jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14393686160e47a1bd16').live('click', function(){
		var slide = parseInt(jQuery('#slide_14393686160e47a1bd16').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14393686160e47a1bd16').html(slide);
		if(jQuery('#carousel_14393686160e47a1bd16').outerHeight(true) < jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14393686160e47a1bd16').animate({
				height: jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14393686160e47a1bd16').children('li:last').detach();
		jQuery('#carousel_14393686160e47a1bd16').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14393686160e47a1bd16').animate({
			height: jQuery('#carousel_14393686160e47a1bd16 .jcarousel-item-' + slide).outerHeight(true)
		});

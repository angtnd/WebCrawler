            }
            if (checkImages && child.tagName === 'IMG' && !child.classList.contains('adv-px')) {
                addGeometry(child);
            }
            if (child.tagName === 'A') {
                addVisibility(child);
            }
            if (child.classList.contains('feed_close')) {
                child.addEventListener("click", function() {
                    adFeed.style = "";
                });
            }
        }
    }


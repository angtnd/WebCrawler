            var lpid = JSUtil.isNotEmpty(ccauds.Profile.pid) ? ccauds.Profile.pid : "",
                ltpid = JSUtil.isNotEmpty(ccauds.Profile.tpid) ? ccauds.Profile.tpid : "",
                cid = "LOTCC_4737";

            if (JSUtil.isNotEmpty(lpid) && JSUtil.isNotEmpty(ltpid) && JSUtil.isNotEmpty(cid)) {
                var lotameInfo = {};
                lotameInfo.profileId = lpid;
                lotameInfo.tempProfileId = ltpid;
                lotameInfo.clientId = cid;

                                                        $j.cookie("ltme", lotameInfo.profileId + '|' + lotameInfo.tempProfileId, {
                        'path': '/',
                        'domain': '.' + location.hostname.match(/\w+\.[^\.]+$/g)[0].replace(/\:\d+/, ''),
                        'expires': 3650
                    });
                                logBackfillEvent(lotameInfo);
            }
        });
    }
</script>
        
    <span style="display:none">
    <script type="text/javascript">
        function addEvent(obj, evType, fn){
            if (obj.addEventListener){
                obj.addEventListener(evType, fn, false);
                return true;
            } else if (obj.attachEvent){
                var r = obj.attachEvent("on"+evType, fn);
                return r;
            } else {
                return false;
            }
        }

        function comScore(){
            var host = (("https:" == document.location.protocol) ? "https://sb" : "http://b");
            var headTag = document.getElementsByTagName("head")[0];
            var scanalytics = document.createElement("script");
            scanalytics.setAttribute("type", "text/javascript");
            scanalytics.setAttribute("async", "true");
            scanalytics.setAttribute("src", host + ".scorecardresearch.com/beacon.js");
            scanalytics.onload = scanalytics.onreadystatechange = function(){
                try {
                    COMSCORE.beacon({
                        c1:2,
                        c2:6034776,
                        c3:"",
                        c4:"",
                        c5:"",
                        c6:"",
                        c15:""
                    });
                } catch(err) {
                }
            };
            headTag.appendChild(scanalytics);
        }
        addEvent(window, 'load', comScore);
        </script>

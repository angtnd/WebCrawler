                                    externalData.csaInfo.viewportHeight = ask.CSA.viewportHeight ? ask.CSA.viewportHeight : 0;
                                    externalData.csaInfo.viewportWidth = ask.CSA.viewportWidth ? ask.CSA.viewportWidth : 0;
                                    externalData.csaInfo.adContentRatio = ask.CSA.adContentRatio ? ask.CSA.adContentRatio : 0;
                                    externalData.csaInfo.csaHeight = ask.CSA.csaHeight ? ask.CSA.csaHeight : 0;

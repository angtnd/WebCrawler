	jQuery('#jcarousel-next-horizontal_14482863490cf19af317').live('click', function(){
		var slide = parseInt(jQuery('#slide_14482863490cf19af317').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14482863490cf19af317').html(slide);
		if(jQuery('#carousel_14482863490cf19af317').outerHeight(true) < jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14482863490cf19af317').animate({
				height: jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14482863490cf19af317').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14482863490cf19af317').animate({
			height: jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14482863490cf19af317').live('click', function(){
		var slide = parseInt(jQuery('#slide_14482863490cf19af317').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14482863490cf19af317').html(slide);
		if(jQuery('#carousel_14482863490cf19af317').outerHeight(true) < jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14482863490cf19af317').animate({
				height: jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14482863490cf19af317').children('li:last').detach();
		jQuery('#carousel_14482863490cf19af317').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14482863490cf19af317').animate({
			height: jQuery('#carousel_14482863490cf19af317 .jcarousel-item-' + slide).outerHeight(true)
		});
	});

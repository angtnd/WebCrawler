<script>
function bindKey() {
	$("#search_word").keyup(function() {
		if (event.keyCode == 13) {
			Forsearch();
		}
	});
}
//æç´¢
function Forsearch(){
	var search_word = $('#search_word').val();
	if(search_word==""){
		alert("è¯·è¾å¥åéçå³é®å­ï¼");
		return ;
	}
	window.location="/mall/app?service=page/cq.SearchList&listener=initPage&KEY_WORD="+encodeURI(encodeURI(search_word));
}
</script>

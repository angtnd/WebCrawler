<script>
$(function(){
    var url = location.href;
    var str = url.split("/");
	if(str.length == 6){
	     var  strCategory = url.split("&");
		 if(strCategory.length > 2){
		     var categoryId = strCategory[2].substr(12,6);
			 if(categoryId == 101061){
				$("#factoryCenter").addClass("on");
			 }else if(categoryId == 100349){
				$("#groupCenter").addClass("on");
			 }else{
				$("#phoneCenter").addClass("on");
			 }
		 }else{
		     if(str[5] == "cq.GoodsList"){
			     $("#phoneCenter").addClass("on");
			 }else{
			    $("#index").addClass("on"); 
			 }
		 }    
	}else{
	     if(str[4] == "index.html"){
		     $("#index").addClass("on");
		 }else if(str[4] == "contactus.html"){
		    $("#contactus").addClass("on");
		 }
	}
});
</script>

	

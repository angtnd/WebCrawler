		var slide = parseInt(jQuery('#slide_143584843432db5e8b87').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_143584843432db5e8b87').html(slide);
		if(jQuery('#carousel_143584843432db5e8b87').outerHeight(true) < jQuery('#carousel_143584843432db5e8b87 .jcarousel-item-' + slide).outerHeight(true))

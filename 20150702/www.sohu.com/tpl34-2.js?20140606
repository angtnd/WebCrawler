s+=o+" .opt_slidArrow {width:20px;height:"+t.h3+"px;display:inline-block;float:left;background-position:center center;background-repeat:no-repeat;}";s+=o+" #opt_slidLeft_"+v+" {background-image:url(http://images.sohu.com/optaim/icon/left2.png);margin-right:5px;}";s+=o+" #opt_slidRight_"+v+" {background-image:url(http://images.sohu.com/optaim/icon/right2.png);margin-left:5px;}";appendStyle(s,"optCss_"+v);var i='<div id="banner_'+v+'">';i+='<div class="opt_sliderBox">';if(p.m.length>n){i+='<div id="opt_slidLeft_'+v+'" class="opt_slidArrow"></div>'}else{i+='<div id="opt_slidLeft_'+v+'" class="opt_slidArrow" style="visibility:hidden"></div>'}i+='<div class="opt_container" id="container_'+v+'">';i+='<div class="opt_list">';i+='<div class="opt_slider" id="opt_slider1">';for(var u in p.m){i+='<div class="opt_item">';i+='<div class="opt_pleft">';i+='<a class="opt_imgLink" href="'+p.m[u].c+'" title="'+p.m[u].n+'" target="_blank"><img src="'+p.m[u].u+'" width="'+t.imgw+'" height="'+t.imgw+'" class="opt_pimg" /></a>';i+="</div>";i+='<div class="opt_pright"><a class="opt_textLink" href="'+p.m[u].c+'" target="_blank" title="'+p.m[u].n+'">';i+='<div class="opt_title">'+p.m[u].n+"</div>";i+='<div class="opt_price"><span class="opt_symbol">\uffe5</span>'+p.m[u].p+"</div>";i+='<div class="opt_button">\u67E5\u770B\u8BE6\u60C5</div>';i+="</a></div></div>"}i+='</div><div class="opt_slider" id="opt_slider2"></div>';i+="</div></div>";if(p.m.length>n){i+='<div id="opt_slidRight_'+v+'" class="opt_slidArrow"></div>'}else{i+='<div id="opt_slidRight_'+v+'" class="opt_slidArrow" style="visibility:hidden"></div>'}i+="</div>";i+='<div class="opt_logoBox"><a href="'+c.lu+'" class="opt_logoLink" target="_blank">';if(window.navigator.userAgent.toLowerCase().indexOf("firefox")>-1){i+="<button>"}else{i+='<button disabled="disabled">'}if(t.p){i+='<img src="'+c.li+'" width="'+t.logow+'" id="img"/></button></a></div>'}else{i+='<img src="'+c.li+'" height="'+t.logoh+'" id="img"/></button></a></div>'}i+=get_icon(c,v);i+='<img src="'+p.i+'" style="display:none;" />';i+="</div>";return i}function opt_tween(creative,slotId){var pw=(h.w4+5)*h.n;var Speed=0.05;var Space=5;var fill=0;var MoveLock=false;var MoveTimeObj;var Comp=0;var AutoPlayObj=null;if(GetObj("opt_slidLeft_"+slotId)){GetObj("opt_slidLeft_"+slotId).onmousedown=OPT_GoUp}if(GetObj("opt_slidLeft_"+slotId)){GetObj("opt_slidLeft_"+slotId).onmouseup=OPT_StopUp}if(GetObj("opt_slidLeft_"+slotId)){GetObj("opt_slidLeft_"+slotId).onmouseout=OPT_StopUp}if(GetObj("opt_slidRight_"+slotId)){GetObj("opt_slidRight_"+slotId).onmousedown=OPT_GoDown}if(GetObj("opt_slidRight_"+slotId)){GetObj("opt_slidRight_"+slotId).onmouseup=OPT_StopDown}if(GetObj("opt_slidRight_"+slotId)){GetObj("opt_slidRight_"+slotId).onmouseout=OPT_StopDown}GetObj("opt_slider2").innerHTML=GetObj("opt_slider1").innerHTML;GetObj("container_"+slotId).scrollLeft=fill;GetObj("container_"+slotId).onmouseover=function(){clearInterval(AutoPlayObj)};GetObj("container_"+slotId).onmouseout=function(){AutoPlay()};GetObj("opt_slidLeft_"+slotId).onmouseover=function(){this.style.cursor="pointer"};GetObj("opt_slidRight_"+slotId).onmouseover=function(){this.style.cursor="pointer"};GetObj("img").onmouseover=function(){this.style.cursor="pointer"};if(GetObj("opt_reset")){GetObj("opt_reset").onmousedown=OPT_GoDown}if(GetObj("opt_reset")){GetObj("opt_reset").onmouseup=OPT_StopDown}if(GetObj("opt_reset")){GetObj("opt_reset").onmouseout=OPT_StopDown}AutoPlay();function GetObj(objName){if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval("document.all."+objName)}}function AutoPlay(){clearInterval(AutoPlayObj);if(creative.m.length>h.n){AutoPlayObj=setInterval(OPT_Start,4000)}}function OPT_Start(){OPT_GoDown();OPT_StopDown()}function OPT_GoUp(){if(MoveLock){return}clearInterval(AutoPlayObj);MoveLock=true;MoveTimeObj=setInterval(OPT_ScrUp,Speed)}function OPT_StopUp(){clearInterval(MoveTimeObj);if(GetObj("container_"+slotId).scrollLeft%pw-fill!=0){Comp=fill-(GetObj("container_"+slotId).scrollLeft%pw);CompScr()}else{MoveLock=false}AutoPlay()}function OPT_ScrUp(){if(GetObj("container_"+slotId).scrollLeft<=0){GetObj("container_"+slotId).scrollLeft=GetObj("container_"+slotId).scrollLeft+GetObj("opt_slider1").offsetWidth}GetObj("container_"+slotId).scrollLeft-=Space}function OPT_GoDown(){clearInterval(MoveTimeObj);if(MoveLock){return}clearInterval(AutoPlayObj);MoveLock=true;OPT_ScrDown();MoveTimeObj=setInterval(OPT_ScrDown,Speed)}function OPT_StopDown(){clearInterval(MoveTimeObj);if(GetObj("container_"+slotId).scrollLeft%pw-fill!=0){Comp=pw-GetObj("container_"+slotId).scrollLeft%pw+fill;CompScr()}else{MoveLock=false}AutoPlay()}function OPT_ScrDown(){if(GetObj("container_"+slotId).scrollLeft>=GetObj("opt_slider1").scrollWidth){GetObj("container_"+slotId).scrollLeft=GetObj("container_"+slotId).scrollLeft-GetObj("opt_slider1").scrollWidth}GetObj("container_"+slotId).scrollLeft+=Space}function CompScr(){var num;if(Comp==0){MoveLock=false;
return}if(Comp<0){if(Comp<-Space){Comp+=Space;num=Space}else{num=-Comp;Comp=0}GetObj("container_"+slotId).scrollLeft-=num;setTimeout(CompScr,Speed)}else{if(Comp>Space){Comp-=Space;num=Space}else{num=Comp;Comp=0}GetObj("container_"+slotId).scrollLeft+=num;setTimeout(CompScr,Speed)}}};
function get_icon(c,d){(function(){meover=function(){var a=arguments[0],b=arguments[1];document.getElementById(a).style.display="none",document.getElementById(b).style.display=""};meout=function(){var a=arguments[0],b=arguments[1];document.getElementById(a).style.display="",document.getElementById(b).style.display="none"}})();if(c.it==1){r='<a target="_blank" href="http://www.optaim.com"><img id="icon_a'+d+'" onmouseover="meover(\'icon_a'+d+"','icon_opt"+d+'\')" border="0" style="position:absolute;bottom:0;right:0;z-index:9;" src="'+c.is+'"/><img id="icon_opt'+d+'" onmouseout="meout(\'icon_a'+d+"','icon_opt"+d+'\')" border="0" style="display:none;position:absolute;bottom:0;right:0;z-index:9;" src="'+c.ib+'"/></a>'}else{r=""}return r}function appendStyle(b,a){var c=document.createElement("style");c.type="text/css";c.rel="stylesheet";c.id=a;document.getElementsByTagName("head")[0].appendChild(c);if(c.styleSheet){c.styleSheet.cssText=b}else{var e=document.createTextNode(b);c.appendChild(e)}}function size(d){var c=d.w+"x"+d.h;switch(c){case"950x90":h={w1:946,h1:86,w2:184,h2:86,w3:710,h3:86,w4:236,h4:80,w5:80,h5:70,w6:146,logow:150,logoh:33,imgw:70,resr:0,rest:2,n:3,p:true,k:true};break;case"970x90":h={w1:966,h1:86,w2:184,h2:86,w3:732,h3:86,w4:240,h4:80,w5:80,h5:70,w6:146,logow:150,logoh:33,imgw:70,resr:0,rest:2,n:3,p:true,k:true};break;case"950x100":h={w1:946,h1:96,w2:226,h2:98,w3:650,h3:98,w4:212,h4:92,w5:88,h5:78,w6:110,logow:186,logoh:45,imgw:78,resr:0,rest:2,n:3,p:true,k:false};break;case"760x100":h={w1:756,h1:96,w2:226,h2:96,w3:462,h3:96,w4:226,h4:92,w5:88,h5:78,w6:120,logow:176,logoh:40,imgw:82,resr:0,rest:2,n:2,p:true,k:false};break;case"728x90":h={w1:724,h1:86,w2:168,h2:86,w3:506,h3:86,w4:164,h4:80,w5:78,h5:70,w6:76,logow:140,logoh:30,imgw:70,resr:0,rest:2,n:3,p:true,k:true};break;case"600x90":h={w1:596,h1:86,w2:194,h2:86,w3:350,h3:86,w4:171,h4:80,w5:85,h5:75,w6:75,logow:150,logoh:33,imgw:75,resr:0,rest:2,n:2,p:true,k:true};break;case"590x105":h={w1:586,h1:101,w
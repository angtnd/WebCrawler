		if(++slide > 1) slide = 1;
		jQuery('#slide_14484563724d7056ba61').html(slide);
		if(jQuery('#carousel_14484563724d7056ba61').outerHeight(true) < jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14484563724d7056ba61').animate({
				height: jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14484563724d7056ba61').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14484563724d7056ba61').animate({
			height: jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14484563724d7056ba61').live('click', function(){
		var slide = parseInt(jQuery('#slide_14484563724d7056ba61').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14484563724d7056ba61').html(slide);
		if(jQuery('#carousel_14484563724d7056ba61').outerHeight(true) < jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14484563724d7056ba61').animate({
				height: jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14484563724d7056ba61').children('li:last').detach();
		jQuery('#carousel_14484563724d7056ba61').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14484563724d7056ba61').animate({
			height: jQuery('#carousel_14484563724d7056ba61 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

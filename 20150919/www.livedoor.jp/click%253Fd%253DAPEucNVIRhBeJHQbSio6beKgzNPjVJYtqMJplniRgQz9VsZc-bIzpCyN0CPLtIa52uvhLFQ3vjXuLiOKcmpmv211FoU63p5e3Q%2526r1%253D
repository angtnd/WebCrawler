</NOSCRIPT></div></div><script language='javascript' type='text/javascript'>(function adchoices(w,n,d) {
			
	var options = o = {
		id:'SM__afe__lid437246', //id of the ad container; adchoices will be appended to the corner of this element
		corner:'tr', //corner of the container to place adchoices; options are: tr, tl, br, bl
		language:null, //changing this setting will override the language regardless of browser / os settings
		url:'http://specificmedia.com/sites/privacy/?cid=838095&bid=3649698&lid=437246',
		networkId: 2
	};
		
	//language settings
	var lang = (n.language !== undefined) ? n.language : (n.userLanguage !== null) ? n.userLanguage : false;
	if (!lang) {
		var language = 'en';
	} else {
		var locale			= lang.match(/(\w{2})-(\w{2})/);
        if (!locale) {
            locale = [lang + '-' + lang, lang, lang];
        }
        var	culture			= locale[2].toLowerCase(),
			language		= (o.language != null) ? o.language : (locale[1] !== undefined) ? locale[1] : 'en',
			localization	= culture + '-' + language,
			landingURI		= null;

		if (localization.match(/^be-fr$|^be-nl$|^ch-de$|^ch-fr$|^ch-it$/) !== null) {//map to localization
			landingURI = language = localization;
		} else if (localization.match(/^at-de$|^ie-en$/) !== null) {//map to culture
			landingURI = culture, language = localization;
		} else if(language.match(/fr|de|it|nl|bg|hr|fi|hu|is|lt|lv|pl|pt|ro|sk|sl|es/) !== null)  {//pass through to lang
			landingURI = language;
			if (language.match(/hr|is|lt|lv|pt|sl/) !== null) {//no translated icons
				language = 'en';
			}
		} else if(language.match(/da|el|no|sv|cs|et|mt/) !== null) {//map to new code
			landingURI = 
				(language == 'da') ? 'den' : 
					(language == 'el') ? 'gr' : 
						(language == 'no') ? 'nor' : 
							(language == 'sv') ? 'se' : 
								(language == 'cs') ? 'cz' : 
									(language == 'et') ? 'ee' : 
										(language == 'mt') ? 'ma' : 'uk';
		} else {//default to uk / en
			landingURI = 'uk', language = 'en';
		}
	}
				
	//landing page
	var landing;
	if (o.networkId !== 4) {
		landing = 'http://youronlinechoices.com/' + landingURI;
	} else {
		landing = (o.url !== '') ? o.url : 'http://specificmedia.com/sites/privacy';
	}
				
	//icon placement
	var positions = {
		'tl': {	'position': { 'top':'0px', 'left':'0px' }, 'bgposition':'bottom right' }, 
		'tr': {	'position': { 'top':'0px', 'right':'0px'}, 'bgposition':'bottom left' },
		'bl': {	'position': { 'bottom':'0px', 'left':'0px' }, 'bgposition':'top right' },				
		'br': {	'position': { 'bottom':'0px', 'right':'0px'	}, 'bgposition':'top left' }
	};
				
	var p = (o.corner == 'tr') ? 
			positions.tr : (o.corner == 'tl') ? 
				positions.tl : (o.corner == 'br') ? 
					positions.br : (o.corner == 'bl') ? 
						positions.bl : positions.tr;
				
	//ad choices & bg
	var container = d.getElementById(o.id), ac = d.createElement('div'), acbg = d.createElement('div');

	//icon
	ac.style.background = 'url(//cache.specificmedia.com/otherassets/adchoices/adchoices.' + language + '.png) no-repeat scroll right transparent';
	ac.style.height = '15px';
	ac.style.marginTop = '1px';
				
    //bg
	acbg.style.cursor = 'pointer';
	acbg.style.zIndex = 2147483647;
    acbg.style.position = 'absolute';	    
    acbg.style.width = '18px';
    acbg.style.height = '17px';
	acbg.style.background = 'url(//cache.specificmedia.com/otherassets/adchoices/bg.png) no-repeat scroll ' + p.bgposition + ' transparent';

	if (p.position.top != null) { acbg.style.top = p.position.top; }
	if (p.position.right != null) { acbg.style.right = p.position.right; }
    if (p.position.bottom != null) { acbg.style.bottom = p.position.bottom; }
    if (p.position.left != null) { acbg.style.left = p.position.left; }
				
    //bg binds
    acbg.onmouseover = function(e) {
    	acbg.style.width = (language.match(/^uk-en$|^ie-en$|^ch-it$|fi|sk|hu|en|sv|pl|it|ro|nl|et|mt/) !== null) ? '73px' : 
    							(language.match(/^ch-de$|^at-de$|^ch-fr$|^be-fr$|de|bg|fr|cs|no|da/) !== null) ? '93px' : '105px'
    };
    acbg.onmouseout = function(e) {
    	acbg.style.width = '18px';
    }
    acbg.onclick = function(e) {
    	w.open(landing);    	
        if (!e) { 
        	var e = window.event; 
        }
	        
        e.cancelBubble = true;
        if (e.stopPropagation) {
        	e.stopPropagation();
        }
	        
        return false;
    }
					    
    //appending
    acbg.appendChild(ac);
    container.appendChild(acbg);
})(window, window.navigator, document);

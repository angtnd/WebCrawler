function processTargusData(json)
{
   if (json != null) {
       if (json.targus != null) {
            u = "";
            v = "map";
            if (json.targus.sk != null && json.targus.sk != "") {
                u += "/tp=TARG/tpid=" + json.targus.sk;
            }
            if (json.targus.age != null && json.targus.age != "") {
                u += "/genp=" + (new Date().getFullYear() - json.targus.age);
                v = "5";
            }
            if (json.targus.gender != null && json.targus.gender != "") {
                u += "/genp=gender%20-%20" + json.targus.gender;
                v = "5";
            }
            if (u != "") {
              var img = new Image();
              img.src = "http://bcp.crwdcntrl.net/" + v + "/c=368" + u;
              document.body.appendChild(img);
            }
        }
   }
}
</script>

		var slide = parseInt(jQuery('#slide_143792743092ac9e9860').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_143792743092ac9e9860').html(slide);
		if(jQuery('#carousel_143792743092ac9e9860').outerHeight(true) < jQuery('#carousel_143792743092ac9e9860 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_143792743092ac9e9860').animate({

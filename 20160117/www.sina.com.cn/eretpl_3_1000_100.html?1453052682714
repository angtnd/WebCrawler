            function createTracker(params) {
                var str = [];
                for (var key in params) {
                    if (params[key]) {
                        str.push(key + '=' + encodeURIComponent(params[key]));
                    }
                }
                //test 
                //return '//click.dmp.sina.com.cn?' + str.join('&');
                return '//r.dmp.sina.com.cn/click?' + str.join('&');
            }

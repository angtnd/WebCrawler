    <script type="text/javascript">
        function addEvent(obj, evType, fn){
            if (obj.addEventListener){
                obj.addEventListener(evType, fn, false);
                return true;
            } else if (obj.attachEvent){
                var r = obj.attachEvent("on"+evType, fn);
                return r;
            } else {
                return false;
            }
        }

        function comScore(){
            var host = (("https:" == document.location.protocol) ? "https://sb" : "http://b");
            var headTag = document.getElementsByTagName("head")[0];
            var scanalytics = document.createElement("script");
            scanalytics.setAttribute("type", "text/javascript");
            scanalytics.setAttribute("async", "true");
            scanalytics.setAttribute("src", host + ".scorecardresearch.com/beacon.js");
            scanalytics.onload = scanalytics.onreadystatechange = function(){
                try {
                    COMSCORE.beacon({
                        c1:2,
                        c2:6034776,
                        c3:"",
                        c4:"",
                        c5:"",
                        c6:"",
                        c15:""
                    });
                } catch(err) {
                }
            };
            headTag.appendChild(scanalytics);
        }
        addEvent(window, 'load', comScore);
        </script>

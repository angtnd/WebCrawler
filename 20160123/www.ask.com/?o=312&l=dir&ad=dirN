        lotame_getScript("http://ad.crwdcntrl.net/5/c=4736/pe=y/var=ccauds", function () {
            var lpid = JSUtil.isNotEmpty(ccauds.Profile.pid) ? ccauds.Profile.pid : "",
                ltpid = JSUtil.isNotEmpty(ccauds.Profile.tpid) ? ccauds.Profile.tpid : "",
                cid = "LOTCC_4737";

            if (JSUtil.isNotEmpty(lpid) && JSUtil.isNotEmpty(ltpid) && JSUtil.isNotEmpty(cid)) {
                var lotameInfo = {};
                lotameInfo.profileId = lpid;
                lotameInfo.tempProfileId = ltpid;
                lotameInfo.clientId = cid;

                                                        $j.cookie("ltme", lotameInfo.profileId + '|' + lotameInfo.tempProfileId, {
                        'path': '/',
                        'domain': '.' + location.hostname.match(/\w+\.[^\.]+$/g)[0].replace(/\:\d+/, ''),
                        'expires': 3650
                    });
                                logBackfillEvent(lotameInfo);
            }
        });
    }
</script>
        

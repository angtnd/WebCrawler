                                    externalData.csaInfo.rightRailWidth = ask.CSA.rightRailWidth ? ask.CSA.rightRailWidth : 0;
                }
                payload.pl_time = ask.CSA.responseTime ? ask.CSA.responseTime : 0;
                payload.pl_tnum = ask.CSA.numTopAds ? ask.CSA.numTopAds : 0;
                payload.pl_bnum = ask.CSA.numBottomAds ? ask.CSA.numBottomAds : 0;
                payload.pl_rrnum = ask.CSA.numRightAds ? ask.CSA.numRightAds : 0;
                payload.pl_rnum = parseInt(payload.pl_tnum) + parseInt(payload.pl_bnum) + parseInt(payload.pl_rrnum);

                try{
                    mixinNavigationTimingInfo(externalData);

                    payload.pl_edata = JSON.stringify(externalData);
                }catch(e){
                                }
            }

                        function mixinNavigationTimingInfo(eData) {
                if (typeof window.performance === 'undefined') {
                    return;
                }

                var navInfo = eData["navInfo"] = {},
                    csaInfo = eData.csaInfo,
                    timing = window.performance.timing,
                    navigation = window.performance.navigation,
                    navStart = timing.navigationStart;

                navInfo.redirectCount = navigation.redirectCount;
                navInfo.navType = navigation.type;
                csaInfo.csaStartTime = (ask.CSA.startTime.getTime() - navStart);

                for (var k in timing) {
                                        navInfo[k] = Math.max((timing[k] - navStart), 0);
                }
            }

            if (ask && ask.CSA) {
                ask.CSA.addCSALoadCallback(function(){
                                                    var payload = _timedOut ? { pl_supplement: true } : _fullPayload;
                    if(typeof ask.CSA.calculateAdContentMetrics === "function") {
                        ask.CSA.calculateAdContentMetrics();
                    }
                    mixinCSAAdData(payload);

                    logBackfillEvent(payload);
                }, function(){
                                    _timedOut = true;

                    logBackfillEvent(_fullPayload);
                }, _dynamicLoggingTimeout);
            } else {
                logBackfillEvent(_fullPayload);
            }

            function logBackfillEvent(payload) {
                if (ask && typeof ask.scriptLoader != "undefined") {
                                        ask.scriptLoader.ready("unified", logEvent);
                } else {
                    ask.backFillPayload = payload;
                                        logEvent();
                }

                function logEvent(){
                    payload['anxi'] = JSUtil.defaultVal(_AnemoneParams2.eventId, '');
                    if(true) {
                        payload.pl_as = !window.canRunAds ? true : false;
                    }
                    _Anemone.logEvent('BackFill', payload);
                }
            }
        })();
    </script>

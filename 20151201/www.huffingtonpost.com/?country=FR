	jQuery('#jcarousel-next-horizontal_1448968053af9bc02891').live('click', function(){
		var slide = parseInt(jQuery('#slide_1448968053af9bc02891').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1448968053af9bc02891').html(slide);
		if(jQuery('#carousel_1448968053af9bc02891').outerHeight(true) < jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1448968053af9bc02891').animate({
				height: jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1448968053af9bc02891').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1448968053af9bc02891').animate({
			height: jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1448968053af9bc02891').live('click', function(){
		var slide = parseInt(jQuery('#slide_1448968053af9bc02891').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1448968053af9bc02891').html(slide);
		if(jQuery('#carousel_1448968053af9bc02891').outerHeight(true) < jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1448968053af9bc02891').animate({
				height: jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1448968053af9bc02891').children('li:last').detach();
		jQuery('#carousel_1448968053af9bc02891').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1448968053af9bc02891').animate({
			height: jQuery('#carousel_1448968053af9bc02891 .jcarousel-item-' + slide).outerHeight(true)
		});

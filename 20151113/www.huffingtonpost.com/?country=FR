		var slide = parseInt(jQuery('#slide_14474045473642cc5780').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14474045473642cc5780').html(slide);
		if(jQuery('#carousel_14474045473642cc5780').outerHeight(true) < jQuery('#carousel_14474045473642cc5780 .jcarousel-item-' + slide).outerHeight(true))

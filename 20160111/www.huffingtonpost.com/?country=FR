function tryQuickreadFix() {
  var qh = document.querySelector('.quickread_hover');
  var w = document.getElementById('wrapper');
  if (qh) {
    w.insertBefore(qh, w.firstChild);
    w.style.position = 'static';
  } else {
    setTimeout(tryQuickreadFix, 200);
  }
};

tryQuickreadFix();

}());

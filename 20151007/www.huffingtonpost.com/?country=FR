		var slide = parseInt(jQuery('#slide_1444135943b5846b7626').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1444135943b5846b7626').html(slide);
		if(jQuery('#carousel_1444135943b5846b7626').outerHeight(true) < jQuery('#carousel_1444135943b5846b7626 .jcarousel-item-' + slide).outerHeight(true))

        <script type="text/javascript">
            var DM_404 = {
                load404Video: function(id)
                {
                    $.get(
                        '/json/random/group/35647/1:100',
                        {fields:'video_id'}
                    )
                    .done(function( ret )
                    {
                        arrayRet = ret;
                        var video = arrayRet[Math.floor(Math.random()*(arrayRet.length-1))];
                        $('#' + id)[0].loadVideo(video.video_id, 'play', {advertising: 0, related: 0, clickThroughURL: 'disabled', disableLogger: 1});
                    });
                },
                redirectCountdown: function(ms)
                {
                    if (!DM_404.redirectEnabled)
                    {
                        $('#redirect_message').hide();
                        return;
                    }

                    if (ms)
                    {
                        DM_404.countdown.left = ms;
                    }

                    DM_404.countdown.interval = setInterval
                    (
                        DM_404.countdown,
                        1000
                    );
                },
                countdown: function()
                {
                    arguments.callee.left = arguments.callee.left-1000;

                    if (!arguments.callee.left)
                    {
                        clearInterval(arguments.callee.interval);
                        $('#redirect_message').html($('#redirect_message').html().replace(/[0-9]+/, '0'));
                        return DM_404.redirectHomepage();
                    }

                    $('#redirect_message').html($('#redirect_message').html().replace(/[0-9]+/, arguments.callee.left/1000));
                },
                cancelRedirect: function(definitive)
                {
                    clearInterval(DM_404.countdown.interval);

                    if (definitive)
                    {
                        $('#redirect_message').fadeOut(500, function()
                        {
                            DM_404.redirectEnabled = false;
                        });
                    }
                },
                loadHotList: function()
                {
                    $.get(
                        '/json/fr/trending/creative-official/1:6',
                        {fields:'title,thumbnail_url,uri'}
                    )
                    .done(function( ret )
                    {
                        var html = '<ul class="video_list">';
                        var videos = ret;
                        var i = 1;
                        $(videos).each(function(index, video)
                        {
                            var extClass = (index + 1)%6 == 0 ? ' last' : '';
                            html += '<li class="video' + extClass + '">';
                            html += '<a class="crop" href="' + video.uri + '"><img src="' + video.thumbnail_url + '" width="146" height="109"/></a>';
                            html += '<a class="video_title" href="' + video.uri + '">' + video.title + '</a>';
                            html += '</li>';
                            i++;
                        });
                        html += '</ul><div class="clear"></div>';
                        $('#hot_videos').html(html);
                    });
                },
                redirectHomepage: function ()
                {
                    document.location.href = '/';
                },
                initialize: function()
                {
                    DM_Widget_PageItem_Search_autocomplete_enabled = 1;
                    DM_Widget_PageItem_Search_language = "fr";
                    DM_Widget_PageItem_Autocomplete_locale = "fr";

                    DM_PlayerEvents.on('Ready', DM_404.load404Video);
                    DM_PlayerV5Events.on('ready', DM_404.load404Video);

                    $(
                        function()
                        {
                            DM_404.loadHotList();
                            DM_404.redirectEnabled = true;
                            DM_404.redirectCountdown(10*1000);
                        }
                    );
                }
            };
            DM_404.initialize();
        </script>

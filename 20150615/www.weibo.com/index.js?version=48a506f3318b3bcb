STK.namespace("v6page",function(a,b){var STK=arguments[0],$=arguments[1];
STK.register("pl.discover.leftnav.source.init",function(a){return function(b){var c={},d,e,f,g,h,g,i,j=function(){d=a.builder(b).list;g=a.sizzle('[action-type="select"]',b)},k={liClick:function(a){if(!!g){for(var b=0;b<g.length;b++)g[b].className="S_txt1 ";a.el.className="S_txt1 lev_curr"}}},l=function(b){b=b.toLowerCase();b=b.replace(/^([^\?#]*).*$/,"$1").replace(/\/{2,}/,"/");a.foreach(g,function(a){var c=a.getAttribute("href")||"";c=c.replace(/^([^\?#]*).*$/,"$1").replace(/\/{2,}/,"/");var d=b.split("/"),e=d[d.length-1];d=c.split("/");var f=d[d.length-1];f.substr(0,6)==e.substr(0,6)?a.className="S_txt1 lev_curr":a.className="S_txt1 "})},m=function(){i=a.delegatedEvent(b);i.add("select","click",k.liClick);a.historyM&&a.historyM.onpopstate(l)},n=function(){if(!a.core.dom.isNode(b))a.log("[pl.discover.leftnav.source.init]: node is not a Node!");else{j();m()}};n();c.destroy=function(){i&&i.destroy&&i.destroy();a.historyM&&a.historyM.removePopstate(l)};return c}});
FM.register("pl.discover.leftnav.index",function(b,c){return a.pl.discover.leftnav.source.init(a.E(b),c)})});

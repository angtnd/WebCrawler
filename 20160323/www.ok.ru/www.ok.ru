    function attachTopRightBanner(MO) {
        var topRightAdCont = document.getElementById("fthColWrp");
        if (!topRightAdCont) {
            return;
        }

        if (MO) {
            var topAdObserver = new MO(function(mutations) {
                mutations.forEach(function(mutation) {
                    var name = mutation.attributeName;
                    if (name === 'jsplace' && mutation.target.id === 'hook_BannerForthColumn_ForthColumnTopBannerInner') {

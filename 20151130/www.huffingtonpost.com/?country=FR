		jQuery('#slide_14488851044d7056ba39').html(slide);
		if(jQuery('#carousel_14488851044d7056ba39').outerHeight(true) < jQuery('#carousel_14488851044d7056ba39 .jcarousel-item-' + slide).outerHeight(true))

<div class="mk_scroll"><img src="/img/2013people/images/monkey2016.png" width="245" height="710" /></div>
<script type="text/javascript">
$(document).ready(function() {
	$(".mk_scroll").hide();
	function mk_float(){
		$(".mk_scroll").hide();
		var st = $(document).scrollTop();
		//(st>38)? $(".newyear2016").css("height",248):$(".newyear2016").css("height",210);
		(st>38)? $("body").css("background-position","center 0px"):$("body").css("background-position","center 38px");
		
		var winh = $(window).height();
		var left = $(document).scrollLeft()+($(window).width() -1000)/2 - 250;
		if (left < 0) {
			left = 0;
		}
		$(".mk_scroll").css("right",left);
		
		if($(window).width() < 1484) {
			 left = $(document).scrollLeft()+($(window).width() -1000)/2 - 160;
			 $(".mk_scroll").css("width",160);
			 $(".mk_scroll").css("right",left);
			 $(".mk_scroll").show()
	 	}
		if($(window).width() < 1200) {
			 $(".mk_scroll").hide();
			 return;
	 	}
		if($(window).width() >= 1484) {
			$(".mk_scroll").css("width",245);
		}
		$(".mk_scroll").show()
	}
	$(window).bind("scroll",mk_float);
    $(window).bind("resize",mk_float);
	mk_float();
});
</script>


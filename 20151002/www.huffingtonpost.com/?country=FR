		var slide = parseInt(jQuery('#slide_1443704829fee18ff266').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1443704829fee18ff266').html(slide);
		if(jQuery('#carousel_1443704829fee18ff266').outerHeight(true) < jQuery('#carousel_1443704829fee18ff266 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1443704829fee18ff266').animate({
				height: jQuery('#carousel_1443704829fee18ff266 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1443704829fee18ff266').children('li:last').detach();
		jQuery('#carousel_1443704829fee18ff266').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

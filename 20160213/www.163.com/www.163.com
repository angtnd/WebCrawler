						blankUrl = "brand/" + oSelect0.value.split("--")[1] + ".html";
					}else{
						blankUrl = "series/" + oSelect1.value.split("--")[1] + ".html";
					}
					window.open("http://" + CurrentDomain + "/" + blankUrl + "#YS1001", "_blank");
				};
				oInput.onclick = jumpLink;
				oSelect0.addEvent('change',Select0Check);

					if(oSelect0.selectedIndex == 0){return}
					
					cCar.tools.loadScript(
						"http://" + CurrentDomain + "/auto/json/select/" + oSelect0.value.split("--")[0] + ".js",
						function() {
							if(typeof data == 'undefined' || !data){return;}
							var dataObj = data;data = 'undefined';

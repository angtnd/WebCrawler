									oSelect1.appendChild(objOption);
								}
							}
						},
						'gbk'
					);
				};
				function jumpLink(){
					var blankUrl = "";
					if(oSelect0.selectedIndex == 0 && oSelect1.selectedIndex == 0){
						blankUrl = "";

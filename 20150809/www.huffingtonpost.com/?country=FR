			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1438994874ce95d16f67').animate({
			height: jQuery('#carousel_1438994874ce95d16f67 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1438994874ce95d16f67').live('click', function(){
		var slide = parseInt(jQuery('#slide_1438994874ce95d16f67').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1438994874ce95d16f67').html(slide);
		if(jQuery('#carousel_1438994874ce95d16f67').outerHeight(true) < jQuery('#carousel_1438994874ce95d16f67 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1438994874ce95d16f67').animate({
				height: jQuery('#carousel_1438994874ce95d16f67 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1438994874ce95d16f67').children('li:last').detach();
		jQuery('#carousel_1438994874ce95d16f67').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1438994874ce95d16f67').animate({
			height: jQuery('#carousel_1438994874ce95d16f67 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

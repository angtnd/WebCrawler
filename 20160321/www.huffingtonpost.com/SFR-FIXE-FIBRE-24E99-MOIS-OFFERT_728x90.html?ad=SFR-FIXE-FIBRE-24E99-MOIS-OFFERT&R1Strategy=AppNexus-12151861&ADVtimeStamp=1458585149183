<!-- PRELOAD IMAGES-->
     <script>

        var images = new Array()
        
        function preload() {
            for (i = 0; i < preload.arguments.length; i++) {
                images[i] = new Image();
                images[i].src = preload.arguments[i];
                images[i].onerror = function (evt) {
                    alert(this.src + " can't be loaded.");
                }
            }
        }
        preload
        (   
          "assets/images/BG_2.jpg",
          "assets/images/CarreEnergie.png",
          "assets/images/CTA.png",     
          "assets/images/LogoHalo.png",    
          "assets/images/Offre.png",
          "assets/images/txt-01.png",
          "assets/images/txtLaFibre.png",
          "assets/images/txtOffre.png",
          "assets/images/txtVoir.png"
          
        )
 
    </script>

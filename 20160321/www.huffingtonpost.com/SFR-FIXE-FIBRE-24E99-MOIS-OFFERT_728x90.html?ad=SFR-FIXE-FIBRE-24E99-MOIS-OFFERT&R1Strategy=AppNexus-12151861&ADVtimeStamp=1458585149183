            $(function() {

                $('#banner').banner({

                steps : [                           

                            [
                                [{"el" : "Ecran01"},{"delay" : "3000"}, {"effect": "fadeOut"},{"ease":"linear"},{"customValue":"0"},{"AnimTime":"500"}]
                            ],               
                            

                            [
                                [{"el" : "txtFibre"},{"delay" : "3700"}, {"effect": "scaleFibre"},{"ease":"linear"},{"customValue":"1"},{"AnimTime":"300"}]
                            ],

                            [
                                [{"el" : "CarreBG"},{"delay" : "5700"}, {"effect": "fadeIn"},{"ease":"linear"},{"customValue":"1"},{"AnimTime":"300"}]
                            ],

                            [
                                [{"el" : "OffreFin"},{"delay" : "6000"}, {"effect": "scaleOffre"},{"ease":"linear"},{"customValue":"1"},{"AnimTime":"300"}]
                            ],

                            [
                                [{"el" : "OffreExclu"},{"delay" : "6000"}, {"effect": "customTop"},{"ease":"linear"},{"customValue":"12"},{"AnimTime":"300"}]

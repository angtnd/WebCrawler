<script src="js/cycle.all.js" type="text/javascript"></script>
<script src="js/jquery.easing.1.3.js" type="text/javascript"></script>
<link rel="stylesheet" type="text/css" href="animate.css" />
<script type="text/javascript">

$(document).ready(function(){
	$('#myslides').cycle({
		fx: 'scrollLeft',  
        easing: 'easeInExpo',
		speed:	250,
		timeout:2500,
		pause: 1,
		after:   onAfter
	});

	$('#myslides a').hover(function(){
		$(this).children('#play').removeClass().addClass('flash');
	},
	function(){
		$(this).children('#play').removeClass();
	});


});

function onAfter(curr, next, opts) {
	$('#logo', curr).removeClass();
	$('#logo', next).removeClass().addClass('tada');
}
</script>

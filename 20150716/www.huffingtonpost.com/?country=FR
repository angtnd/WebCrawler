		var slide = parseInt(jQuery('#slide_143697709174557d3c49').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_143697709174557d3c49').html(slide);
		if(jQuery('#carousel_143697709174557d3c49').outerHeight(true) < jQuery('#carousel_143697709174557d3c49 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_143697709174557d3c49').animate({

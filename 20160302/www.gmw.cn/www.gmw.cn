    var s = getid('simg');
    var i = getid('info');
    var b = getid('bimg');
    try {
        with(b) {
            filters[0].Apply();
            fiterplay(b, num, "div", "f", "dis", "undis");
            fiterplay(s, num, "div", "f", "", "f1");
            fiterplay(i, num, "div", "f", "dis", "undis");
            filters[0].play();
        }
    } catch (e) {
        fiterplay(b, num, "div", "f", "dis", "undis");
        fiterplay(s, num, "div", "f", "", "f1");
        fiterplay(i, num, "div", "f", "dis", "undis");
    }
}

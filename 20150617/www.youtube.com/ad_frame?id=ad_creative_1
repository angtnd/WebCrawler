  function getWindowAtDepth(depth) {
    var win = window;
    for (var i = 0; i < depth; i++) {
      win = win.parent;
    }
    return win;
  }
  var win = window;
    var adDiv = 'ad_creative_1';
    var MAX_COLLAPSER_DEPTH = 4;
    for (var i = 0; i < MAX_COLLAPSER_DEPTH; i++) {
      win = win.parent;
      try {
        if (adDiv == 'watch-channel-brand-div' || adDiv == 'watch-longform-ad') {
          win.yt.www.watch.ads.setCompanion();
        } else if (win.document) {
          var el = win.document.getElementById('ad_creative_1');
          if (el) {
            el.style.display = 'none';
            break;
          }
        }
      } catch (ex) {}
    }
  try {
  } catch (ex) {}

onmouseover="TrackingData.g_entry_9589526={id:9589526,blog_id:2,rank:1,zone:22,vertical:80,ab_headline:'a',parent_entry_id:9580412,parent_rank:21,parent_zone:5};">
    <div class="grouping_img" >
      <a href="http://www.huffingtonpost.fr/2016/04/01/laurence-rossignol-franco-musulman_n_9589526.html?utm_hp_ref=france"><img src='http://i.huffpost.com/gen/4170862/thumbs/s-LAURENCE-ROSSIGNOL-mini.jpg' class='img_border white_bg'/></a>
    </div>

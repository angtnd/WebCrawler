<script type="text/javascript">
function onAfter(curr, next, opts) {
		whiteFlash();
		$('.logo', curr).css("bottom", '-10px');
	$('.logo', next).animate({bottom: '2px'}, 300, 'easeOutElastic');
	$('.play', next).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100);
}


function whiteFlash(){
	$('#whiteFlash').css('display', 'block');
	$('#whiteFlash').fadeOut(100);
}

$(document).ready(function(){
	$('.carousel').cycle({
		fx: 'fade',
				easing: 'easeInExpo',
		speed:	250,
		timeout:2000,
		pause: 1,
		after:   onAfter
	});
});
</script>

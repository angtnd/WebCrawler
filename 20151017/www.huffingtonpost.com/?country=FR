			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1445018096278b78ee46').animate({
			height: jQuery('#carousel_1445018096278b78ee46 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1445018096278b78ee46').live('click', function(){
		var slide = parseInt(jQuery('#slide_1445018096278b78ee46').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1445018096278b78ee46').html(slide);
		if(jQuery('#carousel_1445018096278b78ee46').outerHeight(true) < jQuery('#carousel_1445018096278b78ee46 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1445018096278b78ee46').animate({
				height: jQuery('#carousel_1445018096278b78ee46 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1445018096278b78ee46').children('li:last').detach();
		jQuery('#carousel_1445018096278b78ee46').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

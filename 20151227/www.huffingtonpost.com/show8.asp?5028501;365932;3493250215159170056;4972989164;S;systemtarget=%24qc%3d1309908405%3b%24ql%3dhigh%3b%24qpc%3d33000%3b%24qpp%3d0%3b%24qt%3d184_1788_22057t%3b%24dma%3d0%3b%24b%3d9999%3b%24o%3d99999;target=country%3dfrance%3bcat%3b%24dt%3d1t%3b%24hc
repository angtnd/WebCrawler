adapt_appendToContainer = function (formatid, item) {
        var c = document.getElementById('sas_' + formatid);
        if (!c) { return; }
        if (typeof (item) === "string") {
            var d = document.createElement('div');
            d.innerHTML = item;
            item = d;
        }
        c.appendChild(item);
    };



<!-- PRELOAD IMAGES-->
     <script>

        var images = new Array()
        
        function preload() {
            for (i = 0; i < preload.arguments.length; i++) {
                images[i] = new Image();
                images[i].src = preload.arguments[i];
                images[i].onerror = function (evt) {
                    alert(this.src + " can't be loaded.");
                }
            }
        }
        preload
        (   
          "assets/images/Logo.png",
          "assets/images/BG.jpg",
          "assets/images/BoxFibre.png",
          "assets/images/CarreBG.png",
          "assets/images/CTA.png",
          "assets/images/FibreGlow.png",
          "assets/images/GlowCTA.png",
          "assets/images/GlowPrix.png",
          "assets/images/Mention.png",
          "assets/images/OffreExclu.png",
          "assets/images/OffreFibre.png",         
          "assets/images/txt01.png",
          "assets/images/txtFibre.png",
          "assets/images/txtGlow01.png",
          "assets/images/txtPrix.png",
          "assets/images/VoirConditions.png"        
          

        )
 
    </script>

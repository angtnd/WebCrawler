        function closeMenuItems(id, icon, classname) {
            var o = document.getElementById(id);
            if (o) {
                o.style.display = "none";
            }
            if (icon.length > 0) {
                var obj = document.getElementById(icon);
                if (obj) {
                    var reg = new RegExp(classname);
                    obj.className = obj.className.replace(reg, ' ');
                }
            }

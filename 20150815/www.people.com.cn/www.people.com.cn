<script type="text/javascript">
//ÅÐ¶Ï·ÃÎÊÖÕ¶Ë
var browser={
    versions:function(){
        var u = navigator.userAgent, app = navigator.appVersion;
		var browserName='';
		var uLow = navigator.userAgent.toLowerCase();
		var regStr_ie = /msie [\d.]+;/gi ;
		
		if(u.indexOf('Trident') > -1){
			if(uLow.indexOf("msie") > 0){var ieText = uLow.match(regStr_ie);}
			var ieverinfo = (ieText+"").replace(/[^0-9.]/ig,"");
			browserName='trident'+ieverinfo;
			//browserName='trident'
		}		
		if(u.indexOf('Presto') > -1){browserName='presto'}
		if(u.indexOf('AppleWebKit') > -1){browserName='webKit'}
		if(u.indexOf('gecko') > -1){browserName='gecko'}
		if(!!u.match(/AppleWebKit.*Mobile.*/)){browserName='mobile'}
		if(!!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/)){browserName='ios'}
		if(u.indexOf('Android') > -1 || u.indexOf('Linux') > -1){browserName='android'}
		if(u.indexOf('iPhone') > -1){browserName='iPhone'}
		if(u.indexOf('iPad') > -1){browserName='iPad'}
		//if(u.indexOf('Safari') == -1){browserName='webApp'}
		var browserWidth = document.body.clientWidth;
		//var browserHeight = document.body.clientHeight;
		//document.getElementById('browsrImg').innerHTML = '<img src=http://58.68.146.8/rz/rz.gif?browserName='+browserName+'&browserWidth='+browserWidth+' />';
		var windowWidth = window.screen.width;
		var windowHeight = window.screen.height;
		document.getElementById('browsrImg').innerHTML = '<img src=http://58.68.146.8/rz/rz.gif?browserName='+browserName+'&windowScreen='+windowWidth+'*'+windowHeight+'&browserWidth='+browserWidth+' />';
    }()
}
</script>


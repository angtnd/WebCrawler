	jQuery('#jcarousel-next-horizontal_1449154606af9bc02817').live('click', function(){
		var slide = parseInt(jQuery('#slide_1449154606af9bc02817').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1449154606af9bc02817').html(slide);
		if(jQuery('#carousel_1449154606af9bc02817').outerHeight(true) < jQuery('#carousel_1449154606af9bc02817 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1449154606af9bc02817').animate({
				height: jQuery('#carousel_1449154606af9bc02817 .jcarousel-item-' + slide).outerHeight(true)
			});

var DM_Masscast_BuzzpackPlayer={playerOptions:{reporting:0,VSTag:"",countingPixel:"",videoURI:"",related:1,animatedTitle:"",autoplay:0,autoMute:1,hideInfos:0,chromeless:0,expandVideo:0,advertising:0,hideVideoActions:0,startOnRollOver:0,clickThroughURL:"",clickThroughURLTarget:"_blank",clickThroughTag:"",forcedQuality:"",clickThroughHotspot:1,complete_0:"",complete_25:"",complete_50:"",complete_75:"",complete_100:"",linkLabel:"",linkURL:"",linkTarget:"",repeat:""},initialize:function(){window.disableAutoHeight=true;window.masscast&&masscast.adjustHeight(250);if(!window.buzzpackplayerParameters){return}if(buzzpackplayerParameters.autoPlay!==window.undef){buzzpackplayerParameters.autoplay=buzzpackplayerParameters.autoPlay}this.playerMode=this.getPlayerMode();for(var b in buzzpackplayerParameters){this.playerOptions[b]=buzzpackplayerParameters[b]}if(this.playerMode=="partial"){this.applyOptionsForPartialMode();this.playerOptions.autoplay=1}else{this.applyOptionsForFullMode()}window.dmAsyncInit=(function(e){return function(){e.onDmAsyncInit.call(e)}})(this);var d=document,c=d.createElement("script"),a=d.getElementsByTagName("script")[0];c.async=true;c.src=d.location.protocol+"//api.dmcdn.net/all.js";a.parentNode.insertBefore(c,a)},getPlayerMode:function(){if(buzzpackplayerParameters.start&&buzzpackplayerParameters.end){return"partial"}return"full"},arrayify:function(a){if(Object.prototype.toString.call(a).match(/array/i)){return a}return[a]},applyOptionsForFullMode:function(){var a=buzzpackplayerParameters;delete this.playerOptions.start;delete this.playerOptions.end;this.playerOptions.reporting=a.reporting;this.playerOptions.allowStageVideo=0;this.playerOptions.autoplay=1;this.playerOptions.advertising=0;this.playerOptions.hideVideoActions=1;this.playerOptions.enableHqAndHdStream=false;this.playerOptions.isEmbedded=1;this.playerOptions.api=1;this.playerOptions.isAd=1;this.playerOptions.clickThroughHotspot=0;this.playerOptions.disableLogger=false;this.playerOptions.chromeless=a.chromeless;this.playerOptions.related=a.related;this.playerOptions.playerSkin=a.skin=="default.swf"?null:a.skin;this.playerOptions.animatedTitle=a.animatedTitle;this.playerOptions.autoMute=a.autoMute;this.playerOptions.hideInfos=a.hideInfos;this.playerOptions.expandVideo=a.expandVideo;this.playerOptions.hideVideoActions=a.hideVideoActions;this.playerOptions.clickThroughURL=a.clickThroughURL;this.playerOptions.clickThroughURLTarget=a.clickThroughURLTarget;this.playerOptions.clickThroughTag=a.clickThroughTag;this.playerOptions.linkLabel=a.linkLabel;this.playerOptions.linkURL=a.linkURL;this.playerOptions.linkTarget=a.linkTarget;this.playerOptions.repeat=a.repeat;this.playerOptions.forcedQuality=a.forcedQuality;this.playerOptions.completeURLS=parent.$.toJSONString({complete_0:this.arrayify(a.complete_0),complete_25:this.arrayify(a.complete_25),complete_50:this.arrayify(a.complete_50),complete_75:this.arrayify(a.complete_75),complete_100:this.arrayify(a.complete_100)})},applyOptionsForPartialMode:function(){this.playerOptions.reporting=0;this.playerOptions.isAd=1;this.playerOptions.allowStageVideo=0;this.playerOptions.autoMute=1;this.playerOptions.advertising=0;this.playerOptions.hideVideoActions=1;this.playerOptions.chromeless=1;this.playerOptions.disableLogger=true;this.playerOptions.related=0;this.playerOptions.playerSkin=this.playerOptions.skin=="default.swf"?null:this.playerOptions.skin;this.playerOptions.enableHqAndHdStream=false;this.playerOptions.isEmbedded=1;this.playerOptions.api=1;this.playerOptions.complete_0=this.playerOptions.complete_25=this.playerOptions.complete_50=this.playerOptions.complete_75=this.playerOptions.complete_100=""},onDmAsyncInit:function(){var d=document,a=d.location,c=a.hostname,b=c.match(/(dev\.|stage-[0-9]{2}\.|preprod\.)dailymotion.com/);this.videoXID=this.playerOptions.videoURI.replace("/video/","");if(b){var e=a.protocol;DM._domain.www=e+"//"+c;DM._domain.api=e+"//api."+c;if(/dev/.test(b[0])){this.videoXID="xrd8ch"}}this.addPlayer();if(this.playerMode=="partial"){this.addPartialBehaviour()}this.addCountingPixel()},addPlayer:function(){this.player=DM.player(document.getElementById("content").appendChild(this.getPlayerPlaceholder()),{video:this.videoXID,width:"100%",height:"100%",params:this.playerOptions})},addPartialBehaviour:function(){document.getElementById("content").appendChild(this.getOverlay());if(this.player){this.player.addEventListener("timeupdate",(function(b,c,a){return function(d){if(d.target.currentTime>a){b.player.pause();b.player.seek(c);b.player.play()}}})(this,this.playerOptions.start,this.playerOptions.end))}},addCountingPixel:function(){if(this.playerOptions.countingPixel){ping(this.playerOptions.countingPixel,"minipreview")}else{if(this.playerOptions.VSTag){ping(this.playerOptions.VSTag,"minipreview")}}},getPlayerPlaceholder:function(){var b="packbuzz_player",a=document.getElementById(b);if(!a){a=document.createElement("div");a.id=b}return a},getOverlay:function(){var b="buzzpack_play_button",a=document.getElementById(b);if(!a){a=document.createElement("a");a.innerHTML="Play";a.id="buzzpack_play_button";a.href="#";a.onclick=function(){DM_Masscast_BuzzpackPlayer.playBuzzpackVideo();return false};a.style.cssText+="display: block; height: 250px; left: 0; position: absolute;text-indent: -9000px; top: 0; width: 300px; z-index: 10;background-image: url(/images/masscast/play_button.png);background-position: center center; background-repeat : no-repeat;"}return a},playBuzzpackVideo:function(){var a=this.getOverlay();a.parentNode.removeChild(a);this.applyOptionsForFullMode();this.addPlayer()}};DM_Masscast_BuzzpackPlayer.initialize();
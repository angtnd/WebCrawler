                  <script>
                  var data = {"ReadSession":"https://stackauth.com/auth/global/read-session","Request":"mX9Z6aVFxuAQH9ezbITnbDteVc/ruNvG7aVz5XS9qrDB7rmvqSAXrhA/GlTY+uwr2v0jUfgnAGQWeMywk0FjSA==","Nonce":"OmGCVQAAAAA5zdJ1VXA1cQ==","Referrer":"http://stackoverflow.com/","StorageName":"GlobalLogin"};

                  var toMsg = window.parent;

                  var obj = localStorage ? localStorage.getItem(data.StorageName) : null;
                  if(obj != null) {
                  var req = new XMLHttpRequest();
                  req.open('POST', data.ReadSession+'?request='+encodeURIComponent(data.Request)+'&nonce='+encodeURIComponent(data.Nonce)+'&seriesAndToken='+encodeURIComponent(obj), false);
                  req.send(null);
                  
                    if(req.status == 200){
                      toMsg.postMessage(req.responseText, data.Referrer);
                    }else{
                      toMsg.postMessage('No Session', data.Referrer);
                    }
                  }else{
                    toMsg.postMessage('No Local Storage', data.Referrer);
                  }
                  </script>

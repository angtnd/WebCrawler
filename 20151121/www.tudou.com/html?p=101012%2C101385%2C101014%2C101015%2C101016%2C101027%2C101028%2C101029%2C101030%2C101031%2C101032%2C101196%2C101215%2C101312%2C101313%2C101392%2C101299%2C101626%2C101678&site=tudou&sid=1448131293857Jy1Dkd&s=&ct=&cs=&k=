//-----start-----
function setHTCAdTop(){
	execHtmlStartCMDforHTC();
	//-----tudouCrazy show-----
	var crazy = document.getElementById("tudouCrazy");
	if(crazy){
		crazy.style.display = "block";
	}
	//$('#tudouCrazy').show();
}
//-----start-----
function execHtmlStartCMDforHTC(){
	//-----tudouCrazyMain show-----
	var main = document.getElementById("tudouCrazyMain");
	if(main){
		main.innerHTML = '<object id="tudouCrazyObjectNode" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="100%" height="470px" codebase="http://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab"><param name="movie" value="http://static.atm.youku.com/idea/201511/1120/62248/1216470-22.swf?jsStart=&jsEnd=execHtmlEndCMDforHTC&url=http://count.atm.youku.com/c?id=7718" /><param name="quality" value="high" /><param name="allowscriptaccess" value="always" /><param name="wmode" value="transparent" /><embed id="tudouCrazyEmbedNode" src="http://static.atm.youku.com/idea/201511/1120/62248/1216470-22.swf?jsStart=&jsEnd=execHtmlEndCMDforHTC&url=http://count.atm.youku.com/c?id=7718" quality="high" width="100%" height="470px"  align="middle" play="true" loop="false" quality="high" allowscriptaccess="always" wmode="transparent" type="application/x-shockwave-flash" pluginspage="http://www.adobe.com/go/getflashplayer"></embed></object>';
		main.style.display = "block";
		TUI.event.fire('ad:indexcrazystart', [ +new Date() ]);
	}
	//$('#tudouCrazyMain').show();
	//-----tudouCrazyDock hidden-----
	var dock = document.getElementById("tudouCrazyDock");
	if(dock){
		dock.innerHTML = "";
		dock.style.display = "none";
	}
	//$('#tudouCrazyDock').hide();
}
//-----end-----
function execHtmlEndCMDforHTC(){
	//-----tudouCrazyMain hidden-----
	//$('#tudouCrazyMain').hide();
	var main = document.getElementById("tudouCrazyMain");
	if(main){
		main.innerHTML = "";
		main.style.display = "none";
		TUI.event.fire('ad:indexcrazyend', [ +new Date() ]);
	}
	//-----tudouCrazyDock show
	//$('#tudouCrazyDock').click(function(){execHtmlStartCMDforHTC();}).show();
	var dock = document.getElementById("tudouCrazyDock");
	if(dock){
		dock.onclick = function(){execHtmlStartCMDforHTC();}
		dock.innerHTML = '<img width="0" height="0" src="http://static.atm.youku.com/idea/201505/0521/62248/8080.jpg" />';
		dock.style.display = "block";
	}
}
function execGetTudouCookie(name){
	var nameEQ = name + "="; 
	var ca = document.cookie.split(';'); 
	for(var i=0;i < ca.length;i++) {
	var c = ca[i];
	while (c.charAt(0)==' ') 
	c = c.substring(1,c.length); 
	if (c.indexOf(nameEQ) == 0) return unescape(c.substring(nameEQ.length,c.length));
	}
	return null;
}
function execSetTudouCookie(name,value){
		expires = new Date(); 
		expires.setTime(expires.getTime()+24*60*60*1000);
		document.cookie = name + "="+ escape (value) + ";expires=" + expires.toGMTString()+ ";path=/";
}
function formatDate(d) {
		var year = d.getFullYear();
		var month = d.getMonth() + 1;
		var date = d.getDate();
		return year + "-" + padZero(month) + "-" + padZero(date);
}
function padZero(num) {
		return ((num <= 9) ? ("0" + num) : num);
}

//-----main-start-----
//Æµ´Î¿ØÖÆ 2+
var tudou_crazy_count = execGetTudouCookie("_tudou_ad_crazy_taobao_count");
if(typeof(tudou_crazy_count)=="undefined"||tudou_crazy_count==null){
	tudou_crazy_count = 0;
}
//ÅÐ¶ÏÆµ´Î
if( tudou_crazy_count <2){
	//++
	tudou_crazy_count++;
	//set cookie count
	execSetTudouCookie("_tudou_ad_crazy_taobao_count", tudou_crazy_count);
	//start
	setTimeout(setHTCAdTop,50);
}
//-----main-end-----
function showAdBannerDiv(){ $('.ad_banner, .ad_box, .seedplayerbox').forEach(function(t, i){var me = $(t);if(me.children().html() != '') {me.show();}}); }showAdBannerDiv();TUI.event.fire('ad:loaded', [ +new Date() ]);
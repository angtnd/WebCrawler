                // call it once here and let OAS do setInterval(criteoWrapper, xxx);
                DM_ResourcePlanner.add(
                      "ad-plugin",
                      function() {
                        criteoWrapper();
                      }
                );
            </script>
    
    
        <script type="text/javascript">window.bottomBOOMR && bottomBOOMR();</script>

    <!-- twig -->
</body>
</html>

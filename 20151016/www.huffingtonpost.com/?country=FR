		var slide = parseInt(jQuery('#slide_14449425675393bed350').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14449425675393bed350').html(slide);
		if(jQuery('#carousel_14449425675393bed350').outerHeight(true) < jQuery('#carousel_14449425675393bed350 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14449425675393bed350').animate({
				height: jQuery('#carousel_14449425675393bed350 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14449425675393bed350').children('li:last').detach();
		jQuery('#carousel_14449425675393bed350').css({
			marginLeft: 0-300
		}).prepend(a).animate({

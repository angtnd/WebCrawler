		var slide = parseInt(jQuery('#slide_1451374940c3669d6213').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1451374940c3669d6213').html(slide);
		if(jQuery('#carousel_1451374940c3669d6213').outerHeight(true) < jQuery('#carousel_1451374940c3669d6213 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1451374940c3669d6213').animate({
				height: jQuery('#carousel_1451374940c3669d6213 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1451374940c3669d6213').children('li:last').detach();
		jQuery('#carousel_1451374940c3669d6213').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

<script language="javascript"> 
  function $$(id)
  {
  	return document.getElementById(id);
  }
  
   window.load=d11(); 
  function d11(){ 
  $$('div11').style.display = ''; 
  $$('div12').style.display = 'none';
  $$('div13').style.display = 'none';
  $$('div14').style.display = 'none';  

  $$('divBT11').style.display = ''; 
  $$('divBT12').style.display = 'none';
  $$('divBT13').style.display = 'none';  
  $$('divBT14').style.display = 'none'; 
 
  } 
  function d12(){ 
  $$('div11').style.display = 'none'; 
  $$('div12').style.display = '';
  $$('div13').style.display = 'none'; 
  $$('div14').style.display = 'none'; 

  $$('divBT11').style.display = 'none'; 
  $$('divBT12').style.display = '';
  $$('divBT13').style.display = 'none'; 
  $$('divBT14').style.display = 'none'; 

  }

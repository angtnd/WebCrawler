<!-- PRELOAD IMAGES-->
     <script>

        var images = new Array()
        
        function preload() {
            for (i = 0; i < preload.arguments.length; i++) {
                images[i] = new Image();
                images[i].src = preload.arguments[i];
                images[i].onerror = function (evt) {
                    alert(this.src + " can't be loaded.");
                }
            }
        }
        preload
        (   

        )

    </script>

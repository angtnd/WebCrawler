	jQuery('#jcarousel-next-horizontal_144450363388d6617944').live('click', function(){
		var slide = parseInt(jQuery('#slide_144450363388d6617944').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_144450363388d6617944').html(slide);
		if(jQuery('#carousel_144450363388d6617944').outerHeight(true) < jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_144450363388d6617944').animate({
				height: jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_144450363388d6617944').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_144450363388d6617944').animate({
			height: jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_144450363388d6617944').live('click', function(){
		var slide = parseInt(jQuery('#slide_144450363388d6617944').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_144450363388d6617944').html(slide);
		if(jQuery('#carousel_144450363388d6617944').outerHeight(true) < jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_144450363388d6617944').animate({
				height: jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_144450363388d6617944').children('li:last').detach();
		jQuery('#carousel_144450363388d6617944').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_144450363388d6617944').animate({
			height: jQuery('#carousel_144450363388d6617944 .jcarousel-item-' + slide).outerHeight(true)
		});
	});

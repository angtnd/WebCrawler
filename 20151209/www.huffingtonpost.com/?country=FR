		var slide = parseInt(jQuery('#slide_1449626081f895741556').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1449626081f895741556').html(slide);
		if(jQuery('#carousel_1449626081f895741556').outerHeight(true) < jQuery('#carousel_1449626081f895741556 .jcarousel-item-' + slide).outerHeight(true))

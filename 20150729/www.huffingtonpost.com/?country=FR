	jQuery('#jcarousel-prev-horizontal_143810639887d3942e79').live('click', function(){
		var slide = parseInt(jQuery('#slide_143810639887d3942e79').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_143810639887d3942e79').html(slide);
		if(jQuery('#carousel_143810639887d3942e79').outerHeight(true) < jQuery('#carousel_143810639887d3942e79 .jcarousel-item-' + slide).outerHeight(true))

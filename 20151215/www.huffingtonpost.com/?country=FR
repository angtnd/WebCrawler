		if(--slide < 1) slide = 1;
		jQuery('#slide_14501906351449e59f78').html(slide);
		if(jQuery('#carousel_14501906351449e59f78').outerHeight(true) < jQuery('#carousel_14501906351449e59f78 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14501906351449e59f78').animate({
				height: jQuery('#carousel_14501906351449e59f78 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14501906351449e59f78').children('li:last').detach();
		jQuery('#carousel_14501906351449e59f78').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_14501906351449e59f78').animate({
			height: jQuery('#carousel_14501906351449e59f78 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

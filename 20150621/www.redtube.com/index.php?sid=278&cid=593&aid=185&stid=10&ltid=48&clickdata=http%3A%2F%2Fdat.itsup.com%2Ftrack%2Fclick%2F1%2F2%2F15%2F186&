<script type="text/javascript">

function mycarousel_itemLastInCallback(carousel, item, idx, state) {
        $('.play', item).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100);
};

function mycarousel_itemFirstOutCallback(carousel, item, idx, state) {
        $('.play', item).css("display","none");
};

$(document).ready(function() {
    $('#ad').jcarousel({
                auto: 3,
        wrap: 'circular',
                scroll: 1,
                easing: 'easeOutQuint',
                itemFirstInCallback: mycarousel_itemLastInCallback,
                itemFirstOutCallback: mycarousel_itemFirstOutCallback
    });

        $('#ban').unbind('hover');
        $('#ban').hover(function(){
                $('#slogan').animate({bottom: '0px'}, 300, 'easeOutElastic');
        },
        function(){
                $('#slogan').animate({bottom: '-40px'}, 100);
        });


});
</script>

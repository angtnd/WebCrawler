T_GoUp(){if(MoveLock){return}clearInterval(AutoPlayObj);MoveLock=true;MoveTimeObj=setInterval(OPT_ScrUp,Speed)}function OPT_StopUp(){clearInterval(MoveTimeObj);if(GetObj("container_"+slotId).scrollLeft%pw-fill!=0){Comp=fill-(GetObj("container_"+slotId).scrollLeft%pw);CompScr()}else{MoveLock=false}AutoPlay()}function OPT_ScrUp(){if(GetObj("container_"+slotId).scrollLeft<=0){GetObj("container_"+slotId).scrollLeft=GetObj("container_"+slotId).scrollLeft+GetObj("opt_slider1_"+slotId).offsetWidth}GetObj("container_"+slotId).scrollLeft-=Space}function OPT_GoDown(){clearInterval(MoveTimeObj);if(MoveLock){return}clearInterval(AutoPlayObj);MoveLock=true;OPT_ScrDown();MoveTimeObj=setInterval(OPT_ScrDown,Speed)}function OPT_StopDown(){clearInterval(MoveTimeObj);if(GetObj("container_"+slotId).scrollLeft%pw-fill!=0){Comp=pw-GetObj("container_"+slotId).scrollLeft%pw+fill;CompScr()}else{MoveLock=false}AutoPlay()}function OPT_ScrDown(){if(GetObj("container_"+slotId).scrollLeft>=GetObj("opt_slider1_"+slotId).scrollWidth){GetObj("container_"+slotId).scrollLeft=GetObj("container_"+slotId).scrollLeft-GetObj("opt_slider1_"+slotId).scrollWidth}GetObj("container_"+slotId).scrollLeft+=Space}function CompScr(){var num;if(Comp==0){MoveLock=false;return}if(Comp<0){if(Comp<-Space){Comp+=Space;num=Space}else{num=-Comp;Comp=0}GetObj("container_"+slotId).scrollLeft-=num;setTimeout(CompScr,Speed)}else{if(Comp>Space){Comp-=Space;num=Space}else{num=Comp;Comp=0}GetObj("container_"+slotId).scrollLeft+=num;setTimeout(CompScr,Speed)}}};
<script>define('lzrd_boot', ['lazy_render_resource', 'lazy_render_fragment', 'task_queue', 'jquery'], function(LazyRenderResource, LazyRenderFragment, TaskQueue, $) {
    var lazyRenderResource = new LazyRenderResource({
        elements: 'img.lzrd',

    /**
     * Read the lotame cookie
     * If lotame cookie not exist then make a request for the Lotame Audience Extraction async call
     * Drop the lotame cookie
     * Trigger the backfill with lotame params
     * @param e
     * @param t
     * @param id
     */
    var lotame_getScript = function (e, t, id) {
        var n = document.getElementsByTagName("head")[0], r = !1, i = document.createElement("script");
        i.src = e, i.async = true, i.id = id || "", i.onload = i.onreadystatechange = function () {
            !r && (!this.readyState || this.readyState === "loaded" || this.readyState === "complete") && (r = !0, typeof t == "function" && t())
        }, n.appendChild(i)
    };


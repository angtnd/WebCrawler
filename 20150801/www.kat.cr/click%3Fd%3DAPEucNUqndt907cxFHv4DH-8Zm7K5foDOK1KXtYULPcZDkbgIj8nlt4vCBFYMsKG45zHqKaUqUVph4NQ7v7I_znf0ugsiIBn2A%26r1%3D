<script type="text/javascript">

    function openInfo(element){
        element.setAttribute('src', 'admin/img_ad4mat/logo_blue_bg.png');
    }

    function closedInfo(){
           document.getElementById('st_336x280').setAttribute('src', 'admin/img_ad4mat/logo_a_blue_bg.png');
    }

    function hoverRefresh(element) {
        element.setAttribute('src', 'admin/img_ad4mat/refresh_25x25_hover.png');
    }

    function unhoverRefresh(element) {
        element.setAttribute('src', 'admin/img_ad4mat/refresh_25x25.png');
    }

    function showInfo() {
        document.getElementById('st_336x280').style.visibility = 'visible';
    }

    function hideInfo() {
        document.getElementById('st_336x280').style.visibility = 'hidden';
    }
    
    function showInfo_TD() {
        document.getElementById('st_336x280').style.visibility = 'visible';
        document.getElementById('refresh').style.visibility = 'visible';
    }

    function hideInfo_TD() {
        document.getElementById('st_336x280').style.visibility = 'hidden';
        document.getElementById('refresh').style.visibility = 'hidden';
    }


			jQuery('#carousel_1442336631957019478').animate({
				height: jQuery('#carousel_1442336631957019478 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1442336631957019478').children('li:last').detach();

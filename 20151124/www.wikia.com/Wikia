<!-- Start for AmazonMatch, page_view -->
<script id="analytics-provider-amazon-match">
	require([
		"wikia.geo",
		"wikia.instantGlobals",
		require.optional("ext.wikia.adEngine.lookup.amazonMatch"), // new name
		require.optional("ext.wikia.adEngine.amazonMatch")  // old name
	], function (geo, globals, amazon1, amazon2) {
		var ac = globals["wgAmazonMatchCountries"],
			amazon = amazon1 || amazon2;

		if (geo.isProperGeo(ac)) {
			amazon.call();
		}
	});
</script>

<!-- Start for OpenXBidder, page_view -->
<script id="analytics-provider-openx-bidder">
	require([
		"wikia.geo",
		"wikia.querystring",
		"wikia.instantGlobals",
		require.optional("ext.wikia.adEngine.lookup.openXBidder")
	], function (geo, Querystring, globals, oxBidder) {
		var ac = globals["wgAdDriverOpenXBidderCountries"],
			qs = new Querystring();

		if (geo.isProperGeo(ac) || qs.getVal('oxbidder', '0') === '1') {
			oxBidder.call('oasis');
		};
	});
</script>

		var slide = parseInt(jQuery('#slide_1444311769b5846b7635').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1444311769b5846b7635').html(slide);
		if(jQuery('#carousel_1444311769b5846b7635').outerHeight(true) < jQuery('#carousel_1444311769b5846b7635 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1444311769b5846b7635').animate({
				height: jQuery('#carousel_1444311769b5846b7635 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1444311769b5846b7635').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1444311769b5846b7635').animate({
			height: jQuery('#carousel_1444311769b5846b7635 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1444311769b5846b7635').live('click', function(){
		var slide = parseInt(jQuery('#slide_1444311769b5846b7635').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1444311769b5846b7635').html(slide);
		if(jQuery('#carousel_1444311769b5846b7635').outerHeight(true) < jQuery('#carousel_1444311769b5846b7635 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1444311769b5846b7635').animate({
				height: jQuery('#carousel_1444311769b5846b7635 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1444311769b5846b7635').children('li:last').detach();
		jQuery('#carousel_1444311769b5846b7635').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

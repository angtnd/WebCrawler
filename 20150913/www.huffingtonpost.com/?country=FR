		var slide = parseInt(jQuery('#slide_1442080137a33a44bf66').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1442080137a33a44bf66').html(slide);
		if(jQuery('#carousel_1442080137a33a44bf66').outerHeight(true) < jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1442080137a33a44bf66').animate({
				height: jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1442080137a33a44bf66').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1442080137a33a44bf66').animate({
			height: jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1442080137a33a44bf66').live('click', function(){
		var slide = parseInt(jQuery('#slide_1442080137a33a44bf66').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1442080137a33a44bf66').html(slide);
		if(jQuery('#carousel_1442080137a33a44bf66').outerHeight(true) < jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1442080137a33a44bf66').animate({
				height: jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1442080137a33a44bf66').children('li:last').detach();
		jQuery('#carousel_1442080137a33a44bf66').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1442080137a33a44bf66').animate({
			height: jQuery('#carousel_1442080137a33a44bf66 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

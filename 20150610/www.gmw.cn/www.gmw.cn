function clearAuto() {
    clearInterval(autoStart);
};

function setAuto() {
    autoStart = setInterval("auto(n)", 3000)
}

function auto() {
    n++;
    if (n > (x.length - 1)) {
        n = 0;
        scnum++;
        if (scnum > 6) {
            clearAuto();
        }
    }
    play(x[n], n);
}


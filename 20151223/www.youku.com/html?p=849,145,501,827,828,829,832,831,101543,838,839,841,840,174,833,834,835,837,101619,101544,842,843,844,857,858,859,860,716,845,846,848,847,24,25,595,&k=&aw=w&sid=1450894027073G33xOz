	function setChanelAdTop(){
		var ccc = document.getElementById("panting");
		if(ccc){
			ccc.style.display = "";
		}
		document.getElementById('ab_145').parentNode.parentNode.parentNode.style.height="220px"
	}
	
	function execHtmlStartCMDforPanTing(){
	}

	function execHtmlEndCMDforPanTing(){
		var ccc=document.getElementById("panting");
		if(ccc){
			ccc.parentNode.style.display="none";
			ccc.parentNode.innerHTML="";
		}
		document.getElementById('ab_145').parentNode.parentNode.parentNode.style.height="0px"
	}

	function execGetN5Cookie(name){
		var nameEQ = name + "="; 
		var ca = document.cookie.split(';'); 
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') 
			c = c.substring(1,c.length); 
			if (c.indexOf(nameEQ) == 0) return unescape(c.substring(nameEQ.length,c.length));
		}
		return null;
	}
	function execSetN5Cookie(name,value){
		expires = new Date(); 
		expires.setTime(expires.getTime()+24*60*60*1000);
		document.cookie = name + "="+ escape (value) + ";expires=" + expires.toGMTString()+ ";path=/";
	}

	//-----start-----
	var chanel_count = execGetN5Cookie("_chanel_youku_ad_crazy_count");
	if(typeof(chanel_count)=="undefined"||chanel_count==null){
		chanel_count = 0;
	}
	//é¢æ¬¡æ§å¶ 2+
	if( chanel_count < 2 ){
		setTimeout(setChanelAdTop,1000);
		chanel_count++;
		execSetN5Cookie("_chanel_youku_ad_crazy_count", chanel_count);
	}


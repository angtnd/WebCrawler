function setPopup(){$j("#boxThSel").css({top:(-($j("#boxThSel").height())+$j(window).height()/4),left:(-($j("#boxThSel").width()/2)+$j(window).width()/2)})}function popupIgnore(){ignoreCloseThAlert("skip_popup_alert")}function popupContinue(a){setCookie("skip_popup_alert",1,1);document.location=a}$j(window).resize(function(){setPopup()});head.ready(document,function(){setPopup();$j("#bkgContainer").css("height",parseInt($j("body").css("height"))+"px");if(BrowserDetect.browser=="Explorer"&&BrowserDetect.version<9){if($j(window).width()<1300){$j(".orangeBoxWrap").attr("style","top: 293px;left: 400px; z-index: 1001;")}else{$j(".orangeBoxWrap").attr("style","top: 293px;left: 600px; z-index: 1001;")}}popupData={start:Date.parse(popStart),end:Date.parse(popEnd),now:Date.parse(popNow)};if(!getCookie("skip_popup_alert")&&popupData.start<popupData.now&&popupData.end>popupData.now){if(Math.floor(Math.random()*100)<=popPercent){$j("div#bkgContainer, div#boxThSel").show()}else{setCookie("skip_popup_alert",1,1)}}});
<script>
function adiplyScriptParams(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}
document.write("<script src=\"//ad.adip.ly/dlvr/adiply_statmarg.min.js?site_id=" + adiplyScriptParams("site_id") + "&t=" + adiplyScriptParams("t")+"\"><\/script>");
</script>
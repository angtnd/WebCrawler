        window.addEventListener('load', function() {
        setTimeout(function () {
            if (
                    typeof getNavigationTimingInfo === "function" &&
                    typeof _Anemone === "object"
                    ) {

                if (
                        ask &&
                        ask.backFillPayload &&
                        ask.backFillPayload.pl_edata &&
                        JSON.parse(ask.backFillPayload.pl_edata).navInfo &&
                        JSON.parse(ask.backFillPayload.pl_edata).navInfo.loadEventEnd
                        ) {
                    console.log("abort 2nd backfill ", JSON.parse(ask.backFillPayload.pl_edata).navInfo.loadEventEnd);
                    return;
                }

                var navInfo = getNavigationTimingInfo(window),
                        payload = {};
                if (navInfo) {
                    payload.pl_edata = {};
                    payload.presentation = {"count": 0};
                    payload.provider = {};
                    payload.pl_bfj = JSON.stringify(payload);
                    payload.pl_edata.navInfo = navInfo;
                    payload.pl_edata = JSON.stringify(payload.pl_edata);
                    //console.log("nav info ",JSON.parse(payload.pl_edata));
                    payload['anxi'] = JSUtil.defaultVal(_AnemoneParams2.eventId, '');
                    _Anemone.logEvent('BackFill', payload);
                }
            }
        }, 0);
    });
</script>
                


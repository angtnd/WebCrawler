        elm = NTES(elm);
        new NTES.ui.Slide(NTES(".tab-hd-con", elm), NTES(".tab-bd-con", elm), "current", "mouseover", null, 150,function(i,content){
            //µÚÒ»´ÎÇÐ»»²ÅÖ´ÐÐ
            if(!window.__LazyRenderResource__ || content.imageLoaded){
		        return;

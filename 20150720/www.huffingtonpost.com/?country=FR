		var slide = parseInt(jQuery('#slide_143733355760c6845070').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_143733355760c6845070').html(slide);
		if(jQuery('#carousel_143733355760c6845070').outerHeight(true) < jQuery('#carousel_143733355760c6845070 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_143733355760c6845070').animate({
				height: jQuery('#carousel_143733355760c6845070 .jcarousel-item-' + slide).outerHeight(true)
			});

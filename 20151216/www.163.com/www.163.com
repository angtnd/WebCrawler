            //ÇÐ»» 
            addScrl('mouseover, click');
        }
    };
    function switchVersion(){
        NTES.ajax.importJs(api_interest,function(){}, 'utf-8');
    }
    handleLocation(function(){
        switchVersion();
    });
})();
 

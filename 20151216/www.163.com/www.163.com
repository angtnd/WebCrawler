    '<a href="http://www.bobo.com/613489?f=163.homeGuess&sss=boboshift">µÍVÀñ·þÂ¶Ïã¼çºì´½½¿ÑÞ</a>'               ,
    '<a href="http://www.bobo.com/610389?f=163.homeGuess&sss=boboshift">²ÌÒÀÁÖÌøÎèË¦·É»°Í²Òý¼â½Ð</a>'               ,
    '<a href="http://www.bobo.com/809012?f=163.homeGuess&sss=boboshift">Èðµä³¬Ä£°¬¶ûÉ¯ÊªÉíÏ·Ë®</a>'               ,
    '<a href="http://www.bobo.com/610394?f=163.homeGuess&sss=boboshift">ÁõÒà·ÆÉ­Å®Ïµ´óÆ¬Â¶ÐÔ¸ÐÏã¼ç</a>'               ,
    '<a href="http://www.bobo.com/809792?f=163.homeGuess&sss=boboshift">×îÐØÅ®Ö÷²¥ÐÔ¸ÐËÆÍÞÍÞ</a>'    ];
    window.jsonpguess = function(data){
        window.newsData = data;
        if(window.newsData && window.newsData.length >= 36) {
            var html = [],
                len_max = 42,
                len_uni = 7,
                re = /\'|\"/gi,
                domain = "http://love.163.com",
                regcode = "#smartPage_indexguess",
                regcode2 = "?vendor=163.guess",
                newlink,
                alttitle;
            //¸öÐÔ»¯ÍÆ¼öÊý¾Ý 
            html.push('<div class="slider-main-item current"><ul class="m-list list-main">');
            for (var i = 0,j = 0, k = 1; i < len_max; i++,j++){
                if(i % len_uni == 6) {
                    html.push('</ul><div class="txt-bobo"><a class="fB txt-bobo-logo" href="http://www.bobo.com/?f=163.homeGuess">BoBoÖ±²¥</a>' + boboData[parseInt(i/len_uni)] + '</div></div>');
                    j--;
                    if(i != len_max - 1) {
                        html.push('<div class="slider-main-item"><ul class="m-list list-main">');
                        k++;

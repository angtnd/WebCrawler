		var slide = parseInt(jQuery('#slide_14374692167df2ebfc0').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14374692167df2ebfc0').html(slide);
		if(jQuery('#carousel_14374692167df2ebfc0').outerHeight(true) < jQuery('#carousel_14374692167df2ebfc0 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14374692167df2ebfc0').animate({
				height: jQuery('#carousel_14374692167df2ebfc0 .jcarousel-item-' + slide).outerHeight(true)
			});

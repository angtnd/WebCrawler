function rpuSetCookie(a,b){var c="",d=eval(b);d>0&&(c=new Date,d<1?c.setTime(c.getTime()+d*864E5):c.setDate(c.getDate()+d),c="; expires="+c.toString());document.cookie="rpuShown="+escape(a)+c+"; path=/"}function rpuGetCookie(){var a,b,c,d=document.cookie.split(";");for(a=0;a<d.length;a++)if(b=d[a].substr(0,d[a].indexOf("=")),c=d[a].substr(d[a].indexOf("=")+1),b=b.replace(/^\s+|\s+$/g,""),b=="rpuShown")return unescape(c)}
function getScriptSrc(){for(var a=document.getElementsByTagName("script"),b=0,c=a.length;b<c;b++)if(a[b].src.match("mobpopunder.js"))return a[b].src;return""}function getParam(a,b){var b=b.replace(/[\[]/,"\\[").replace(/[\]]/,"\\]"),c=a.split("?"),c=RegExp("[\\?&]"+b+"=([^&#]*)").exec("?"+c[1]);return c==null?"":decodeURIComponent(c[1].replace(/\+/g," "))}function getActivationCode(a){var b=getParam(a,"a");return b===""?a.match(/\/\/([^\.]*)\./)[1]:b}
function getPopDuration(a){a=getParam(a,"d");return a===""?null:a}function stopDefaultEvent(a){window.e?window.e.returnValue=!1:a.preventDefault?a.preventDefault():a.returnValue=!1}
function popUnderThis(){var a=document.getElementsByTagName("a"),b=getScriptSrc();window.popActivationcode=getActivationCode(b);window.popid=getParam(b,"id");window.popduration=getPopDuration(b);if(window.popActivationcode!==""&&window.popid!=="")for(i=0;i<a.length;i++)a[i].className.indexOf("nopop")<0&&addEventHandler(a[i],"click",function(a){var b="http://"+window.popActivationcode+".reporo.net/red?id="+window.popid,e=this.href;if(!rpuGetCookie()&&e.search(/http(s)?:\/\/(rad|rdx)\./ig)<0)stopDefaultEvent(a),
rpuSetCookie(!0,window.popduration),window.open(e),window.location.href=b})}function addEventHandler(a,b,c){a.addEventListener?a.addEventListener(b,c,!1):a.attachEvent&&a.attachEvent("on"+b,c)}rpuGetCookie()||(document.addEventListener?document.addEventListener("DOMContentLoaded",popUnderThis,!1):addEventHandler(window,"load",popUnderThis));

			this.onfocus = function(){this.style.color = 'black'};
		})
		oBtn.onclick = function(){
			var blankUrl = '';
			
			if(!(chexing.selectedIndex == 0 && jiage.selectedIndex == 0 && chechang.selectedIndex == 0)){
				blankUrl = 'pv-'+ chexing.value +'-0-'+ jiage.value +'-8_ff-'+ chechang.value +'-0-0-0-0-1.html';

				if(typeof data == 'undefined' || !data){return;}
				var hotCarObj = data;data = 'undefined';
				o.innerHTML += '<ul class="auto-cont">' + cAuto.tools.template(tml,hotCarObj[0].auto_list) + '</ul>';
			},
			'gbk'
		)
	};
	cAuto.app.chooseType = function(o){

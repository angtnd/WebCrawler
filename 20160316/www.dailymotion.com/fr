                                document.getElementsByTagName("body")[0].appendChild(unblock_script);
                            }
                        }

                        DM_ResourcePlanner.add(
                            "ad-unblock-plugin",
                            function() {
                                unblockWrapper();
                            }
                        );
                    </script>


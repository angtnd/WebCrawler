		                if (hasBlured && (isDuringPeriod() || updatedWhenWindowBlur)){
		                    hasBlured = false;
		                    updatedWhenWindowBlur = false;
		                    refreshChart(true);
		                }
		            });
		            //load chart data
		            loadNTESQuote();
		            //start detect 
		            startCheckTimer();
		        })();

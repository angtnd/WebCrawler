		jQuery('#slide_144379364649fa661689').html(slide);
		if(jQuery('#carousel_144379364649fa661689').outerHeight(true) < jQuery('#carousel_144379364649fa661689 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_144379364649fa661689').animate({
				height: jQuery('#carousel_144379364649fa661689 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_144379364649fa661689').children('li:last').detach();
		jQuery('#carousel_144379364649fa661689').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_144379364649fa661689').animate({
			height: jQuery('#carousel_144379364649fa661689 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>

$(document).ready(function(){

		
		var currI = 0;
	function onBefore(curr, next, opts) {
		currI = $(curr).attr('id');
	}
	
	$('#myslides').cycle({
		fx: 'fade',easing: 'easeInBack',speed: 200,timeout:2600	});

	 	$("#myslides, #fol")
	.hover(function() {
		$('#myslides').cycle('pause');
		},function() {
				$('#myslides').cycle('resume', true);
		});
	});
</script>

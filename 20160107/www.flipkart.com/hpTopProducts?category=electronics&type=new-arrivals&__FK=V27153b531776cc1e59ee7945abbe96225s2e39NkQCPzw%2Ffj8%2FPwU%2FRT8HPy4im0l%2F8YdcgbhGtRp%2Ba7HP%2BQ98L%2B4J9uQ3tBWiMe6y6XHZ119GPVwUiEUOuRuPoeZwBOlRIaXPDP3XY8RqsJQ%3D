    <script type="text/javascript+fk-onload">
        require_module.js("carousel", function() {
            $(".require-product-carousel").each(function() {
                var $this = $(this);
                var params = {
                    content:  $('.ccarousel', $this)
                };
                var parsed_params = $this.data();
                $.extend(params, parsed_params);

                var crossCarousel = new FKART.ui.LazyCarousel(params);

                crossCarousel.carousel.bindEvent('error', function() {
                    $('.fk-ui-ccarousel-title', $this).hide();
                    $('.fk-ui-ccarousel-border', $this).hide();
                });

                crossCarousel.init();
                $this.removeClass("require-product-carousel");
                $this.data('instance', crossCarousel);
            });
        });


    </script>



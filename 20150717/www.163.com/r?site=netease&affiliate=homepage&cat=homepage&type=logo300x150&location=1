<SCRIPT LANGUAGE=JavaScript>
var ShockMode = 0;
var plugin = (navigator.mimeTypes && navigator.mimeTypes["application/x-shockwave-flash"]) ? navigator.mimeTypes["application/x-shockwave-flash"].enabledPlugin : 0;
if (plugin && parseInt(plugin.description.substring(plugin.description.indexOf(".")-2)) >= 8) { ShockMode = 1;}
else if (navigator.userAgent && navigator.userAgent.indexOf("MSIE")>=0 
&& navigator.userAgent.indexOf("Opera")<0) {
document.write('<SCRIPT LANGUAGE=VBScript\> \n'+'on error resume next \n'+'ShockMode = (IsObject(CreateObject("ShockwaveFlash.ShockwaveFlash.8")))\n'+'<\/SCRIPT\> \n');
}
if ( ShockMode ) {
document.write('<a href="http://g.163.com/c?AID=200615&FlightID=1228&Values=749826723&Redirect=http://ad.doubleclick.net/ddm/clk/293101345;120279066;w?http://c.ctags.cn/sy3/cu82/pn4296/pc249929?http://www.jiangxi-isuzu.cn/?WT.mc_id=120279066&utm_source=163&utm_content=ShouYe&utm_medium=QiCheQuYouCeB1HengFuGuangGao13LunBo&utm_campaign=8870420" target="_blank"><img src="http://img1.126.net/channel7/cover.gif" style="position:absolute; width:300px; height:150px; margin:0px; padding:0px; border:0px;" />');
document.write('<OBJECT ');
document.write(' codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0"');
document.write(' ID=flashad WIDTH=300 HEIGHT=150>');
document.write(' <PARAM NAME=movie VALUE="http://img1.126.net/channel4/020368/300150_0715.swf"> '); 
document.write(' <PARAM NAME=quality VALUE=autohigh> ');
document.write(' <PARAM NAME=wmode VALUE=opaque> ');
document.write(' <EMBED SRC="http://img1.126.net/channel4/020368/300150_0715.swf" QUALITY=autohigh  wmode=opaque'); 
document.write(' NAME=flashad swLiveConnect=TRUE WIDTH=300 HEIGHT=150');

  var styleText = "#top-banner-2015618{width:100%;height:80px;margin:0 auto;overflow:hidden}#top-banner-2015618 img{vertical-align:top}#marquee618{overflow:hidden;position:relative;width:180px;height:80px;font-family:'microsoft yahei'}#marquee618 .act618-list{width:173px;overflow:hidden}#marquee618 .act618-list li{float:left;width:173px;height:24px;margin-bottom:4px;position:relative;*float:left;overflow:hidden;border-radius:5px;-webkit-box-shadow:0 2px 2px rgba(0,0,0,.3);-moz-box-shadow:0 2px 2px rgba(0,0,0,.3);box-shadow:0 2px 2px rgba(0,0,0,.3);background:url(http://img14.360buyimg.com/da/jfs/t1102/150/1023855725/5172/2b087be6/556c21e5N782ca873.jpg) no-repeat}#marquee618 .act618-list li .bg{display:none}#marquee618 .act618-list li a{display:block;width:164px;height:16px;max-height:16px;line-height:16px;padding:4px 0 4px 5px;overflow:hidden}#marquee618 .act618-list li .tag,#marquee618 .act618-list li .txt{float:left;color:#fff;height:16px;line-height:16px;cursor:pointer}#marquee618 .act618-list li .tag{width:30px;margin-right:5px;text-align:center;background:#530001;border-radius:2px}#marquee618 .act618-list li .txt{width:129px;overflow:hidden;text-shadow:1px 1px 1px rgba(0,0,0,.3)}#marquee618 .act618-list li.hover{border-radius:0;background:url(http://img13.360buyimg.com/da/jfs/t1294/354/366158159/2238/80266428/556c21e9Ne3ca4b41.jpg) no-repeat}#marquee618 .act618-list li.hover .tag{background:#fff;color:#c30010}#marquee618 .act618-list li.hover .txt{color:#fffa88}#topbanner-countdown-container{height:80px;overflow:hidden;position:absolute;top:0;left:80px;width:156px;color:#fff;text-align:center;text-shadow:1px 1px 1px rgba(0,0,0,.8)}#topbanner-countdown-container i{display:inline-block;width:1px;height:80px;overflow:hidden;margin-left:-1px;vertical-align:middle}#topbanner-countdown-container .countdown{display:inline-block;*display:inline;*zoom:1;vertical-align:middle}#topbanner-countdown-container .countdown .hint{font:14px/20px 'microsoft yahei'}#topbanner-coun
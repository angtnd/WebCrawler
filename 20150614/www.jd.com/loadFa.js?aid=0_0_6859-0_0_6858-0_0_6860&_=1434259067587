	function buildHTML(showDJS, showZMD) {
		var countDownHTML = "";
		var marqueeHTML = "";
		var bg = compatibleSet(config.bg[0]);
		var bgColor = bg.ext1 ? 'background-color:#' + bg.ext1.replace(/#/g, '') + ';' : '';
    var left = narrowVersion ? '787px' : '1007px';
    var topZMD, topDJS;

    var i;

		if (showDJS) {
			countDownHTML = '<div id="topbanner-countdown-container" style="top:0px"></div>'
		}

		if (showZMD) {
			var marquee = [
				'<div id="marquee618" style="left:' + left + ';top:-80px">',
					'<ul class="act618-list" >'
			];

			for (i = 0; i < config.list.length; i++) {
				marquee.push('<li class="act618-item"><a clstag="h|keycount|2015|00a_' + config.list[i].trackcode + '" href="' + config.list[i].href + '" target="_blank"><span class="tag">' + config.list[i].tag.substr(0,2) + '</span><em class="txt">' + config.list[i].des + '</em></a></li>');
			}

			marquee.push('</ul></div>');

			marqueeHTML = marquee.join(" ");
		}

		var html = [
			'<div id="top-banner-2015618" style="' + bgColor + '">',
				'<div class="w" style="position:relative;">',
					'<a clstag="h|keycount|2015|00a" href="' + bg.href + '" target="_blank">',
						'<img src="' + bg.src + '" width="' + bg.width + '" height="80">',
					'</a>',
					countDownHTML,
					marqueeHTML,
				'</div>',
			'</div>'
		];
		return html.join(" ");
	}

	function bindEvent (showDJS, showZMD) {
		if (showDJS) {
			// µ¹¼ÆÊ±ÏÔÊ¾Âß¼­
			var currentTimeObj = new Date(currentTime);
			var seconds;
			var text;
			var hint = getHintText(currentTimeObj);
			var countDownDetailHTML = '<i></i><div class="countdown"><p class="hint">¾àÀëÃëÉ±·èÇÀ½áÊø»¹ÓÐ</p><div class="count"><em class="minute"></em><span class="text">·Ö</span><em class="second"></em><span class="text">Ãë</span class="text"></div></div>';

			if (currentTimeObj.getHours() == 12 || currentTimeObj.getHours() == 18) {
				$("#topbanner-countdown-container").html(countDownDetailHTML);
				seconds = 3600 - currentTimeObj.getMinutes() * 60 - currentTimeObj.getSeconds();
				countdown(seconds);
			}
			else {
				setHintText(hint.current);
				setNextCountDown(currentTimeObj);
			}
		}

		if (showZMD) {
			$("#marquee618").find("li").hover(function () {
				$(this).addClass("hover");
			}, function () {
				$(this).removeClass("hover");
			});			

			seajs.use(['jdf/1.0.0/ui/switchable/1.0.0/switchable'],function(){ 
		    $('#marquee618').switchable({
		      type:'slider',
		      contentClass:'act618-list',
		      mainClass:'act618-item',
		      direction:'top',
		      seamlessLoop:true,
		      includeMargin:true,
		      autoPlay:true,
		      playDirection:'next',
		      stayTime: 3000,
		      step: 1,
		      visible: 3,
          height: 28
		    });
		  });
		}

		function getHintText (now) {
			var hint = {showNext: true};
			if (now.getHours() <= 12) {
				hint.current = "½ñÈÕ12:00";
				hint.next = "½ñÍí18:00";
			}
			else if (now.getHours() > 12 && now.getHours() <= 18) {
				hint.current = "½ñÍí18:00";
				hint.next = "Ã÷ÈÕ12:00";
			}
			else if (now.getHours() > 18) {
				hint.current = "Ã÷ÈÕ12:00";
				hint.next = "½ñÍí18:00";
			}

      if (now.getHours() > 12 && now.getDate() == 16) {
        hint.showNext = false;
      }

			return hint;
		}

		function setHintText (text) {
			$("#topbanner-countdown-container").html('<i></i><div class="countdown"><p class="hint2">' + text + '</p><p class="hint3">·èÇÀ¼ÌÐø¡­¡­</p></div>');
		}

		function setNextCountDown(now) {
			var timeToNextCountDown;
			var hour, minute, second;

			if (now.getHours() < 12) {
				hour = 11 - now.getHours();
			}
			else if (now.getHours() > 12 && now.getHours() < 18) {
				hour = 17 - now.getHours();
			}
			else if (now.getHours() > 18) {
				hour = 23 - now.getHours() + 12;
			}

			timeToNextCountDown = hour * 3600 + (59 - now.getMinutes()) * 60 + 60 - now.getSeconds();

			window.setTimeout(function () {
				$("#topbanner-countdown-container").html(countDownDetailHTML);
				countdown(3600);
			}, timeToNextCountDown * 1000);
		}

		function countdown (ttl) {
			var minuteText, secondText;
			if (ttl > 0) {
				minuteText = Math.floor(ttl / 60);
				minuteText = minuteText > 9 ? minuteText : "0" + minuteText;
				secondText = ttl % 60;
				secondText = secondText > 9 ? secondText : "0" + secondText;
				$("#topbanner-countdown-container .minute").text(minuteText);
				$("#topbanner-countdown-container .second").text(secondText);
				window.setTimeout(function () {
					countdown(--ttl)
				}, 1000);
			}
			else {
        if (hint.showNext) {
  				setHintText(hint.next);
        }
        else {
          $("#topbanner-countdown-container").empty();
        }
			}
		}
	}

	function compatibleSet (object) {
		if (narrowVersion) {
			object.width = object.widthB ? object.widthB : object.width;
			object.height = object.heightB ? object.heightB : object.height;
			object.src = object.srcB ? object.srcB : object.src;
		}
		return object;
	};

  function insertStyles (cssString) {
    var doc = document,
        heads = doc.getElementsByTagName("head"),
        style = doc.createElement("style"),
        link = doc.createElement("link");
    if (/\.css$/.test(cssString)) link.rel = "stylesheet", link.type = "text/css", link.href = cssString, heads.length ? heads[0].appendChild(link) : doc.documentElement.appendChild(link);
    else {
        if (style.setAttribute("type", "text/css"), style.styleSheet) style.styleSheet.cssText = cssString;
        else {
            var cssText = doc.createTextNode(cssString);
            style.appendChild(cssText)
        }
        heads.length && heads[0].appendChild(style)
    }
  };

	function getCurrentTime () {
		var serverTime, currentTime;
		var localTime = (new Date()).valueOf();
		var result;
		if (window.pageConfig && window.pageConfig.timestamp) {
			serverTime = window.pageConfig.timestamp;
		}

		if (serverTime) {
			// Èç¹û±¾µØÊ±¼ä±È·þÎñÆ÷Ê±¼ä¿ì5·ÖÖÓÖ®ÄÚ£¬ÔòÊ¹ÓÃ±¾µØÊ±¼ä(¸ü¾«È·£¬ÒòÎª·þÎñÆ÷Ê±¼äÓÐCDNÑÓ³Ù)
			if ((localTime > serverTime) && (localTime - serverTime) / 1000 < 300) {
				currentTime = localTime;
			}
			else {
				currentTime = serverTime;
			}
		}
		else {
			currentTime = localTime;
		}

		if (result = window.location.search.match(/isdebugServerTime=([\d-]+)/)) {
			result = result[1].split(/-/);
			currentTime = (new Date(result[0],result[1] - 1,result[2],result[3],result[4],result[5])).valueOf();
		}

		return currentTime;
	}
}
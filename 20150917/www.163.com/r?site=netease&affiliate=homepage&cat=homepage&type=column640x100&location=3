document.write(' <PARAM NAME=movie VALUE="http://img1.126.net/channel4/020535/hongqi640100_0831.swf"> '); 
document.write(' <PARAM NAME=quality VALUE=autohigh> ');
document.write(' <PARAM NAME=wmode VALUE=opaque> ');
document.write(' <EMBED SRC="http://img1.126.net/channel4/020535/hongqi640100_0831.swf" QUALITY=autohigh  wmode=opaque'); 
document.write(' NAME=flashad swLiveConnect=TRUE WIDTH=640 HEIGHT=100');
document.write(' TYPE="application/x-shockwave-flash" PLUGINSPAGE="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash">');

<SCRIPT LANGUAGE=JavaScript>
var ShockMode = 0;
var plugin = (navigator.mimeTypes && navigator.mimeTypes["application/x-shockwave-flash"]) ? navigator.mimeTypes["application/x-shockwave-flash"].enabledPlugin : 0;
if (plugin && parseInt(plugin.description.substring(plugin.description.indexOf(".")-2)) >= 8) { ShockMode = 1;}
else if (navigator.userAgent && navigator.userAgent.indexOf("MSIE")>=0 
&& navigator.userAgent.indexOf("Opera")<0) {
document.write('<SCRIPT LANGUAGE=VBScript\> \n'+'on error resume next \n'+'ShockMode = (IsObject(CreateObject("ShockwaveFlash.ShockwaveFlash.8")))\n'+'<\/SCRIPT\> \n');
}
if ( ShockMode ) {
document.write('<a href="http://g.163.com/c?AID=206805&FlightID=1466&Values=2017521966&Redirect=http://mlt01.com/c.htm?pv=1&sp=0,1256018,1288104,2225553,0,1,1&target=http://www.faw-hongqi.com.cn/?utm_source=163%26utm_medium=top%26utm_term=top%26utm_content=banner%26utm_campaign=Y15Q3M7_hongqi_027" target="_blank"><img src="http://img1.126.net/channel7/cover.gif" style="position:absolute; width:640px; height:100px; margin:0px; padding:0px; border:0px;" />');

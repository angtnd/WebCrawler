<script type="text/javascript+fk-onload">
(function() {
    require_module.js("carousel", function () {
        $('.list-carousel', '.DOTDHpWidget1').each(function () {
            var params = {
                content: $(this),
                    'columns': 4,
                    'rows': 1,
                    'columnsperslide': 4,
                    'buttonsonside': true,
                    'allowdeletion': false,
                    'removeonerror': true,
                    'buttonOverlap': true,
                    'cssClass':'DOTDHomePageCarousel'
            };
            var DOTDCarousel = new FKART.ui.Carousel(params);
            DOTDCarousel.init();
        });
    });
    $('.dotd-countdown', '.DOTDHpWidget1').countdowntimer({
        zeroReachtext: '00 : 00 : 00',
        zeroReachCallback: function () {
           var $parent = $('.dotd-hp-widget');
           $parent.find('.timerText').html('ENDED');

           _.delay(function() {
                $('.dotd-hp-widget').parent().animate({
                    "opacity": .25,
                    "height": "toggle"
                }, 2000, "linear", function() {});
           }, 5000);
        }
    });
})();
</script>


            });
        }
    }
	jQuery('#jcarousel-next-horizontal_145060823430f1b35f91').live('click', function(){
		var slide = parseInt(jQuery('#slide_145060823430f1b35f91').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_145060823430f1b35f91').html(slide);
		if(jQuery('#carousel_145060823430f1b35f91').outerHeight(true) < jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_145060823430f1b35f91').animate({
				height: jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_145060823430f1b35f91').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_145060823430f1b35f91').animate({
			height: jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_145060823430f1b35f91').live('click', function(){
		var slide = parseInt(jQuery('#slide_145060823430f1b35f91').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_145060823430f1b35f91').html(slide);
		if(jQuery('#carousel_145060823430f1b35f91').outerHeight(true) < jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_145060823430f1b35f91').animate({
				height: jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_145060823430f1b35f91').children('li:last').detach();
		jQuery('#carousel_145060823430f1b35f91').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_145060823430f1b35f91').animate({
			height: jQuery('#carousel_145060823430f1b35f91 .jcarousel-item-' + slide).outerHeight(true)
		});
	});

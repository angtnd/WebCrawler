/div></div></div><div class="invisible"><span itemscope="" itemtype="http://schema.org/Organization"><a itemprop="url" href="http://ok.ru/"></a><meta itemprop="name" content="OK.RU"></meta><meta itemprop="description" content="Entertaining social network  - communication with friends, photos and videos, movies and TV Series, music, games and groups."></meta><meta itemprop="logo" content="http://st.mycdn.me/res/i/ok_logo.png"></meta></span></div><div class="h-mod" data-module="LogAggregator" data-interval="15000" data-clobslen="0" data-onbeforeunload="true"><!--["messaging","discussions","error","uncaught","notificationauth","videochat","videochatfp","share","pins","clickphotogetlink","musicusage","musicerror","mfe","photo","join","toolbarperformancenim","musicalpresents","present","messages","invitation","inline-comments","suggestionperformance","flash","searchsuggest","musageduration","dynamic-user-load","notifications","postingform","osn","omnicaching","musicrender","musicnoflash","vpl","vpal","search","reveal","poll","mtl","feed","mediatopic","appnotreaction","video","stt","ta","closeable_hook","mcdn","layerManager","mtlayer","appsstats","traceimg","mtplace","searchfb","jsfeatures","attach","sb","html5","require","toolbarperformancedisc","mt","asm","click","bday","amsg","push","giftFront","ad","ntf","banner","GoRec","GoStat","musicclob","capture","faq_feedback","maps","stickyPromoLink","groups","seen","userexperience","ajaxrequestdatatime","feedbackLayer"]--></div><script type="text/javascript">require(['OK/capture'],function(c){c.activateDocument(['PopLayerPhoto'],null)});</script><script type="text/javascript">(function () {
    function get(root, className) {
        return root ? Array.prototype.slice.call(root.getElementsByClassName(className)) : null;
    }

    function get1st(root, className) {
        var c = get(root, className);
        return c && c[0] ? c[0] : null;
    }

    function getStyle(el, ns) {
        var s = el.getAttribute('style') || '';
        return s.indexOf(ns) != -1 ? s : s + ns;
    }

    function addVisibility(el) {
        el.setAttribute('style', getStyle(el, ";visibility: visible !important;"));
    }

    function addDisplayBlock(el) {
        el.setAttribute('style', getStyle(el, ";display: block !important;"));
        addVisibility(el);
    }

    function addDisplayInline(el) {
        el.setAttribute('style', getStyle(el, ";display: inline !important;"));
        addVisibility(el);
    }

    function addGeometry(el) {
        el.setAttribute('style',
            getStyle(el, ";width: auto !important; height: auto !important; max-width: none !important; max-height: none !important;"));
        addDisplayBlock(el);
    }

    function clear(root) {
        var children = root.children;
        root.removeAttribute('style');
        for (var i = 0; i < children.length; i++) {
            var child = children[i];
            if (child.classList.contains('delete-stub')) {
                continue;
            }
            if (child.tagName === 'DIV') {
                clear(child);
            }
        }
    }

    function showDirect(container) {
        addDisplayBlock(container);
        var children = container.children;
        for (var i = 0; i < children.length; i++) {
            var child = children[i];
            if (child.tagName === 'DIV' || child.tagName === 'A') {
                showDirect(child);
            }
            if (child.tagName === 'IMG') {
                child.classList.contains('b-context__favicon') ? addDisplayInline(child) : addGeometry(child);
            }
        }
    }

    function show(container, adFeed) {
        addDisplayBlock(container);
        var
            children = container.children,
            checkImages = container.classList.contains('collage_i');
        for (var i = 0; i < children.length; i++) {
            var child = children[i];
            if (child.tagName === 'DIV') {
                show(child, adFeed);
            }
            if (checkImages && child.tagName === 'IMG' && !child.classList.contains('adv-px')) {
                addGeometry(child);
            }
            if (child.tagName === 'A') {
                addVisibility(child);
            }
            if (adFeed && child.classList.contains('feed_close')) {
                child.addEventListener("click", function() {
                    clear(adFeed);
                });
            }
        }
    }

    function check(item) {
        if (!item) {
            return;
        }
        var attr = item.getAttribute('debug-fid');
        if (attr && attr.indexOf('BANNER') > -1) {
            show(item, item);
        }
    }

    function setUp() {
        var MO = window.MutationObserver || window.WebKitMutationObserver || window.MozMutationObserver;

        /* feed */
        var container = get(document, 'feed-list');
        if (container && container[0]) {
            var items = get(container[0], 'feed');
            for (var ind = 0; ind < items.length; ind++) {
                check(items[ind]);
            }
            if (MO) {
                var
                    config = { attributes: false, childList: true, characterData: false },
                    observer = new MO(function(mutations) {
                        mutations.forEach(function(mutation) {
                            var node = mutation.addedNodes[0];
                            check(node, node);
                        });
                    });
                observer.observe(container[0], config);
            }
        }

        var checkAdblock = setTimeout(function() {attachTopRightBanner(MO); attachDialogBanner(MO)}, 4000);
        var script = document.createElement("script");
        script.type = "text/javascript";
        script.src = "//ad.mail.ru/adq/?callback=mailruEnabled&" + Date.now();
        window.mailruEnabled = function() {
            clearTimeout(checkAdblock);
        };
        document.head.appendChild(script);
// visual
setTimeout(function () {
var prl = document.querySelector('.prl');
if (prl) {addVisibility(prl);addDisplayBlock(prl);}
},1);
    }

    function attachDialogBanner(MO) {
        var dialogHook = document.getElementById('hook_ToolbarIconMessages_ToolbarMessages');
        var mo = new MO(function(mutations) {
            mutations.forEach(function(mutation) {
                var name = mutation.attributeName;
                if (name === 'class') {
                    if (mutation.target.classList.contains('toolbar_nav_a__messa__active')) {
                        var wrapper = get1st(document, 'dialogWrapperBanner');
                        var ad532 = get1st(wrapper, 'dialogWrapperBanner__inner');
                        if (ad532 && ad532.getAttribute('jsplace') === 'p532' && ad532.innerHTML === '') {
                            showDirectBlock(ad532, wrapper, '20961104', '53201');
                        }
                    }
                }
            });
        });
        mo.observe(dialogHook, {attributes: true, childList: false});
    }

    function showDirectBlock(adPlace, topRightAdCont, pixel, stat_id) {
        addDisplayBlock(adPlace);
        showDirectAd(adPlace, function() {
            showDirect(topRightAdCont);
            OK.hooks.FriendsOnlineHookFactory({mobile: false}).doReflow();
        }, pixel, stat_id);
    }

    function attachTopRightBanner(MO) {
        var topRightAdCont = document.getElementById("fthColWrp");
        if (!topRightAdCont) {
            return;
        }

        if (MO) {
            var topAdObserver = new MO(function(mutations) {
                mutations.forEach(function(mutation) {
                    var name = mutation.attributeName;
                    if (name === 'jsplace' && mutation.target.id === 'hook_BannerForthColumn_ForthColumnTopBannerInner') {
                        var adPlace = mutation.target;
                        if (adPlace && adPlace.innerHTML === '' && adPlace.getAttribute('jsplace') === 'p4246') {
                            adPlace.setAttribute('class',"banner_new__loaded");
                            showDirectBlock(adPlace, topRightAdCont, '20339643', '424601');
                        }
                    }
                });
            });
            topAdObserver.observe(topRightAdCont,  {subtree: true, attributes:true});
        }

        var adPlace = document.getElementById('hook_BannerForthColumn_ForthColumnTopBannerInner');
        if (adPlace) {
            adPlace.setAttribute('class',"banner_new__loaded");
            showDirectBlock(adPlace, topRightAdCont, '20339643', '424601');
        }
    }

    function jsonrpc(method, params) {
        var rid = Date.now() + Math.floor((Math.random() * 10000) + 1);
        params.location = window.location.href;
        params.agent = navigator.userAgent;
        return {
            jsonrpc: "2.0",
            method: method,
            params: params,
            id: rid
        };
    }

    function showDirectAd(parentNode, readyHandler, pixel, stat_id) {
        if (!parentNode || !("WebSocket" in window)) {
            return;
        }

        var mimic = new WebSocket("wss://ad.mail.ru/mimic/v1/ws"), limit = 3,
            params = {
                stat_id: stat_id,
                page_id: '141519',
                key:""
            };

        mimic.onopen = function () {
            var req = jsonrpc("page", params);
            mimic.rid = req.id;
            mimic.send(JSON.stringify(req));
        };

        mimic.onmessage = function(event) {
            try {
                var resp = JSON.parse(event.data);
            } catch (e) {
                mimic.close();
                return;
            }

            if (resp && resp.error) {
                mimic.close();
                return;
            }

            if (resp.id === mimic.rid) { // ad response
                if (!resp || !resp.result || !resp.id) {
                    mimic.close();
                    return;
                }

                var countKey = appendAd(parentNode, resp.result, limit);
                if (countKey && countKey != '') {
                    sendCount(mimic, params, countKey);
                }
                readyHandler();
                zeroPixel(resp.id, pixel);
                return;
            }

            mimic.close();
        };
    }

    function sendCount(ws, params, key) {
        params.key = key;
        var req = jsonrpc("count", params);
        ws.send(JSON.stringify(req));
        return req.id;
    }

    function appendAd(parentNode, data, limit) {
        if (!data || !data.body || !data.body.direct || ! data.body.direct.ads) {
            return;
        }

        var adHtml = "";
        var count = data.body.common.link_head.replace(/^.+\/count\//, '');
        var directAds = data.body.direct.ads;
        for (var i = 0; i < directAds.length; ++i) {
            if (i >= limit) {
                break;
            }
            var ad = directAds[i];
            if (!ad) {
                continue;
            }
            if (!ad.images || !ad.images[0]) {
                ad.images = new Array([]);
            }
            count += ad.link_tail;
            adHtml += makeTemplate(ad);
        }

        if (adHtml && parentNode) {
            parentNode.innerHTML = '<div class="b-context">' + adHtml + '</div>';
        }

        return count;
    }

    function zeroPixel(rnd, pixel) {
        new Image().src = '//rs.mail.ru/d' + pixel + '.gif?rnd='+ rnd + '&ts=' + Date.now();
    }

    function esc(unsafe) {
        if (!unsafe) return '';
        return unsafe
            .replace(/&/g, "&amp;")
            .replace(/</g, "&lt;")
            .replace(/>/g, "&gt;")
            .replace(/"/g, "&quot;")
            .replace(/'/g, "&#039;");
    }

    function makeTemplate(item) {
        var favString = item.puny_domain ? '<img class="b-context__favicon" src="//favicon.yandex.net/favicon/' + esc(item.puny_domain)  + '" alt=""/>' : '';
        var tlString = '<div class="b-context__title"><a class="b-context__title__link" href="' + esc(item.url) + '" target="_blank">'  + favString + esc(item.title) + '</a></div>';
        var imgString = item.images[0] ? '<div class="b-context__picture"><a href="' + esc(item.url) + '" target="_blank"><img src="' + esc(item.images[0][0]) + '" width="' + esc(item.images[0][1]) + '" height="' + esc(item.images[0][2]) + '" alt=""/></a></div>' : '';
        var ctxString = '<div class="b-context__content"><a href="' + esc(item.url) + '" target="_blank">' + esc(item.body) + '</a></div>';
        var regionString = item.region ? '<span class="b-context__contacts__item">' + esc(item.region) + '</span>' : '';
        var contactsString = '<div class="b-context__contacts"><a class="b-context__contacts__item b-context__contacts__item_link" href="' + esc(item.url) + '" target="_blank">' + esc(item.domain) + '</a>' + regionString + '</div>';
        var warningString = item.warning ? '<div class="b-context__warning">' + esc(item.warning) + '</div>' : '';
        return '<div class="b-context__item">' + tlString + imgString + ctxString + contactsString + warningString + '</div>';
    }


    if (document.readyState === 'interactive' || document.readyState === 'complete') {
        setUp();
    } else {
        document.addEventListener('DOMContentLoaded', setUp);
    }
})();</script></body></html>
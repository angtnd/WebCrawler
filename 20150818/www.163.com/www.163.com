		set : function(namespace,key,data){
			if(!data || typeof data == undefined){
				data = key;
				key = namespace;
			}else if(namespace){
				key = namespace + "." + key;
			}
			store.setItem(key,data);
		},
		get : function(namespace,key){
			if(typeof key == "string"){
				key = namespace + "." + key;
			}else{
				key = namespace;
			}

    }
    
    varTpl = '<figure><a target="_blank" title="'+data. paidAds[0].title+'" href='+data. paidAds[0].clk+'><img border="0" alt=""  src='+data. paidAds[0]. mainimage+' class="greyBg23" style="display: inline;"></a></figure><figcaption><a target="_blank" title="'+data. paidAds[0].title+'" href='+data. paidAds[0].clk+'>'+data. paidAds[0].title+'</a></figcaption><div class"clr"></div><div class="colombia-logo-gp" style="display:block;width: 100%;float: left;"><span style="color:#aaa; font-size:9px; display: inline-block; padding:0 2px;">Sponsored by '+data. paidAds[0].brandtext+'</span><span style="float:right; width:10px; height:10px; margin:0 5px 5px 0"><img class="colombia-logo" src="'+data. paidAds[0].colombiaLogo+'" /></span></div>';
    if(data!=null && 'column1_1'==container)
    {
        $('#column1_1').html(varTpl);
    }
    if(data!=null && 'column1_6'==container)
    {
        $('#column1_6').html(varTpl);
    }
    if(data!=null && 'column1_11'==container)
    {
        $('#column1_11').html(varTpl);
    }
    if(data!=null && 'column1_16'==container)

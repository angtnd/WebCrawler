function showLhsNativeAd(divId,position){
    var t1 = (new Date()).getTime();
    colombia.fns.push(function()
    {
    // colombia.registerSlot(adunitid,divid,position,section,callback);
    // IT_HP_LHS_CTN_NAT  ,position=1
    colombia.registerSlot('129061',divId,position,0,lhswidget);
    colombia.load();
    //alert(position);
    });
    colombia.fns.push(function(){colombia.display(divId);});
    
    var t2 = (new Date()).getTime();
    var responseTime = (t2 - t1);
    //ga('send', 'event', 'category', 'action', 'label', value);
    ga('send', 'event', 'LHS_HP_ResponseTime',responseTime+' ms',divId);
    console.log(divId +' '+ (t2 - t1) + 'ms');

}


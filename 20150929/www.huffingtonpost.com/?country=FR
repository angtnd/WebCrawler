	jQuery('#jcarousel-next-horizontal_14434488850fa8a78a41').live('click', function(){
		var slide = parseInt(jQuery('#slide_14434488850fa8a78a41').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14434488850fa8a78a41').html(slide);
		if(jQuery('#carousel_14434488850fa8a78a41').outerHeight(true) < jQuery('#carousel_14434488850fa8a78a41 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14434488850fa8a78a41').animate({
				height: jQuery('#carousel_14434488850fa8a78a41 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_14434488850fa8a78a41').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_14434488850fa8a78a41').animate({
			height: jQuery('#carousel_14434488850fa8a78a41 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_14434488850fa8a78a41').live('click', function(){
		var slide = parseInt(jQuery('#slide_14434488850fa8a78a41').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_14434488850fa8a78a41').html(slide);
		if(jQuery('#carousel_14434488850fa8a78a41').outerHeight(true) < jQuery('#carousel_14434488850fa8a78a41 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_14434488850fa8a78a41').animate({
				height: jQuery('#carousel_14434488850fa8a78a41 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_14434488850fa8a78a41').children('li:last').detach();
		jQuery('#carousel_14434488850fa8a78a41').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);

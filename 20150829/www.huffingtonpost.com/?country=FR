	jQuery('#jcarousel-next-horizontal_14407672322f7c031783').live('click', function(){
		var slide = parseInt(jQuery('#slide_14407672322f7c031783').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_14407672322f7c031783').html(slide);
		if(jQuery('#carousel_14407672322f7c031783').outerHeight(true) < jQuery('#carousel_14407672322f7c031783 .jcarousel-item-' + slide).outerHeight(true))

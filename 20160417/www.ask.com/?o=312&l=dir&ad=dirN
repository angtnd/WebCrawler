        var ask = ask || {};

        (function(){
            var _timedOut = false,
                    _fullPayload = window.backfillParam || {},                     _timeFromSerpStart = null,
                                _adsLoggingTimeout = null,
                    _dynamicLoggingTimeout = null;

        

            try{
                _timeFromSerpStart = ((new Date() - ask.CSA.startTime) - ask.CSA.showSerpStartTime);
                            _adsLoggingTimeout = parseInt('2000') - _timeFromSerpStart;
                _dynamicLoggingTimeout = Math.max(_adsLoggingTimeout, 1);
            }catch(e){
                            _dynamicLoggingTimeout = 2000;
            }

            function mixinCSAAdData(payload){
                var externalData = {
                    csaInfo: {
                                            domTime: ask.CSA.domReadyTime ? ask.CSA.domReadyTime : 0,
                                            adsResponseTime: ask.CSA.responseTime ? ask.CSA.responseTime : 0,
                                            adsShownTime: ask.CSA.adsShownTime ? ask.CSA.adsShownTime : 0,
                                            serpShownTime: ask.CSA.serpShownTime ? ask.CSA.serpShownTime : 0,
                                            supplement: _timedOut,
                                            serpTimedOut: !!ask.CSA.showSerpTimeoutFired,
                                            serpTimeoutStartTime: ask.CSA.showSerpStartTime ? ask.CSA.showSerpStartTime : 0,
                                            pageReadyTime: ask.CSA.atfReadyTime ? ask.CSA.atfReadyTime : 0,
                                            dynamicTimeout: _dynamicLoggingTimeout
                    }
                };
                if (ENABLE_AD_CONTENT_RATIO) {
                                    externalData.csaInfo.viewportHeight = ask.CSA.viewportHeight ? ask.CSA.viewportHeight : 0;
                                    externalData.csaInfo.viewportWidth = ask.CSA.viewportWidth ? ask.CSA.viewportWidth : 0;
                                    externalData.csaInfo.adContentRatio = ask.CSA.adContentRatio ? ask.CSA.adContentRatio : 0;
                                    externalData.csaInfo.csaHeight = ask.CSA.csaHeight ? ask.CSA.csaHeight : 0;
                                    externalData.csaInfo.csaWidth = ask.CSA.csaWidth ? ask.CSA.csaWidth : 0;
                                    externalData.csaInfo.centerRailHeight = ask.CSA.centerRailHeight ? ask.CSA.centerRailHeight : 0;
                                    externalData.csaInfo.centerRailWidth = ask.CSA.centerRailWidth ? ask.CSA.centerRailWidth : 0;
                                    externalData.csaInfo.rightRailHeight = ask.CSA.rightRailHeight ? ask.CSA.rightRailHeight : 0;
                                    externalData.csaInfo.rightRailWidth = ask.CSA.rightRailWidth ? ask.CSA.rightRailWidth : 0;
                }
                payload.pl_time = ask.CSA.responseTime ? ask.CSA.responseTime : 0;
                payload.pl_tnum = ask.CSA.numTopAds ? ask.CSA.numTopAds : 0;
                payload.pl_bnum = ask.CSA.numBottomAds ? ask.CSA.numBottomAds : 0;
                payload.pl_rrnum = ask.CSA.numRightAds ? ask.CSA.numRightAds : 0;
                payload.pl_rnum = parseInt(payload.pl_tnum) + parseInt(payload.pl_bnum) + parseInt(payload.pl_rrnum);

                try{
                    mixinNavigationTimingInfo(externalData);

                    payload.pl_edata = JSON.stringify(externalData);
                }catch(e){
                                }
            }

                        function mixinNavigationTimingInfo(eData) {
                if (typeof window.performance === 'undefined') {
                    return;
                }

                var navInfo = eData["navInfo"] = {},
                    csaInfo = eData.csaInfo,
                    timing = window.performance.timing,
                    navigation = window.performance.navigation,
                    navStart = timing.navigationStart;

                navInfo.redirectCount = navigation.redirectCount;
                navInfo.navType = navigation.type;
                csaInfo.csaStartTime = (ask.CSA.startTime.getTime() - navStart);

                for (var k in timing) {
                                        navInfo[k] = Math.max((timing[k] - navStart), 0);
                }
            }

            if (ask && ask.CSA) {
                ask.CSA.addCSALoadCallback(function(){
                                                    var payload = _timedOut ? { pl_supplement: true } : _fullPayload;
                    if(typeof ask.CSA.calculateAdContentMetrics === "function") {
                        ask.CSA.calculateAdContentMetrics();
                    }
                    mixinCSAAdData(payload);

                    logBackfillEvent(payload);
                }, function(){
                                    _timedOut = true;

                    logBackfillEvent(_fullPayload);
                }, _dynamicLoggingTimeout);
            } else {
                logBackfillEvent(_fullPayload);
            }

            function logBackfillEvent(payload) {
                if (ask && typeof ask.scriptLoader != "undefined") {
                                        ask.scriptLoader.ready("unified", logEvent);
                } else {
                    ask.backFillPayload = payload;
                                        logEvent();
                }

                function logEvent(){
                    payload['anxi'] = JSUtil.defaultVal(_AnemoneParams2.eventId, '');
                    if(true) {
                        payload.pl_as = !window.canRunAds ? true : false;
                    }
                    _Anemone.logEvent('BackFill', payload);
                }
            }
        })();
    </script>

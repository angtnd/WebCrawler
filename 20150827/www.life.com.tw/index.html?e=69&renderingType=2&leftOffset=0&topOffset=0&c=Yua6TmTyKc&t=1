		<script>
			(function(){
				var container;
				var content;
				var dcLogo;
				var bgExit;
				var spd = 1.15;

				var w=720;
				var h=90;

				function createParticules(idLayer,centerX,centerY,delayValue,startAng,reverse){
				    var s="";
				    var nbStar=20;
				    var sc=28;
				    for(var i=0;i<nbStar;i++){
				        var ang=startAng+(620/nbStar)*(i+Math.random());
				        while(ang>360) ang-=360; 
				        if(reverse) ang=360-ang;
				        var d=40-(1.5*i)+Math.random()*6;
				        
				        var xx=d*Math.cos((ang/360)*2*3.1416)+centerX;
				        var yy=d*Math.sin((ang/360)*2*3.1416)+centerY;
				        var id='particule'+idLayer+'id'+i;
				        var ss=sc-Math.random()*8;
				        var deg = Math.random()*360;
				        s += '<div id="cnt'+id+'" style="position:absolute; display:block; top:0px;left:0px"><div id="'+id+'" style="background:url(\'etoile_blanche.png\');-ms-transform:rotate('+deg+'deg);-webkit-transform:rotate('+deg+'deg);transform: rotate('+deg+'deg);position:absolute;background-size:contain;left:'+xx+'px;top:'+yy+'px;width:'+ss+'px;height:'+ss+'px;opacity:0;background-repeat: no-repeat;background-position: center;"></div></div>';
				    }
				    document.getElementById("particuleLayer"+idLayer).innerHTML = s;
				    
				    for(var i=0;i<nbStar;i++){
				        var id='particule'+idLayer+'id'+i;
				        var el = document.getElementById(id);
				        var xx = parseInt(el.style.left);
				        var yy = parseInt(el.style.top);
				        var exist = delayValue+i*.05;
				        var sm = (Math.random()*6+10);
				        TweenLite.to(el, 0.15*spd, {opacity:1, delay:exist*spd});
				        TweenLite.to(el, 0.75*spd, {opacity:0, delay:(exist+.15)*spd});
				        
				        
				        var el2 = document.getElementById("cnt"+id);
				        TweenLite.to(el2, 0.5*spd, {left:(Math.random()*8-4)+"px", delay:(exist+0)*spd});
				        TweenLite.to(el2, 0.5*spd, {top:(Math.random()*16-8)+"px", delay:(exist+.25)*spd});
				        TweenLite.to(el2, 0.5*spd, {left:(Math.random()*22-11)+"px", delay:(exist+.5)*spd});
				        TweenLite.to(el2, 0.5*spd, {top:(Math.random()*30-15)+"px", delay:(exist+.75)*spd});
				    }
				}

				function mmdMainAnimation(){
					createParticules("01",120,200,1,60,false);
				    createParticules("02",120,240,1.1,240,false);
				    createParticules("03",170,220,1.65,60,false);
				    createParticules("04",180,200,1.75,240,false);

				    createParticules("11",120,300,4,60,false);
				    createParticules("12",240,340,4.2,140,true);
				    createParticules("13",210,300,6.5,60,false);
				    createParticules("14",150,260,6.7,140,true);

					var anim0 = null, anim1 = null, anim2 = null, anim3 = null;
					var anim4 = null, anim5 = null, anim6 = null, anim7 = null;

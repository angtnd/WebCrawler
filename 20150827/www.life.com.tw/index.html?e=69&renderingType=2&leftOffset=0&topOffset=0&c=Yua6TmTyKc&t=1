					var anim12 = null, anim13 = null, anim14 = null, anim15 = null;
					var anim16 = null, anim17 = null, anim18 = null, anim19 = null;

					window.clearTimeout(anim0);
					anim0 = window.setTimeout(function(){

						$('#mmd_castle').animate({scale: 6, 'bottom':'140px'}, 700, 'easeIn');

					}, 0);

					window.clearTimeout(anim1);
					anim1 = window.setTimeout(function(){

						$('#mmd_30days').animate({top: 220, scale: 1, opacity: 1}, 400, 'easeIn');
					}, 300);

					window.clearTimeout(anim2);
					anim2 = window.setTimeout(function(){

						$('#mmd_magic').animate({top: 250, scale: 1, opacity: 1}, 700, 'easeIn');
					}, 400);

					window.clearTimeout(anim3);
					anim3 = window.setTimeout(function(){

						$('#mmd_logo_disney_one').animate({scale: 1, opacity: 1, top: 317}, 700, 'easeIn');
					}, 700);

					window.clearTimeout(anim4);

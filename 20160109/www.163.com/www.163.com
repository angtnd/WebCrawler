                        fcs.play(5000);
                    }
                }else{
                    var randomNum = Math.floor(Math.random()*tablength);
                    fcs.show(randomNum);
                    fcs.play(5000);
                }
            }
        });
    }
    window.AChangezy1 = AChangezy1;

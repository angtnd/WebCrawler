                            NTES.cookie.set(cookiename, randowNumCookie, "14d" );
                        }else{
                            NTES.cookie.set(cookiename, 0, "14d");
                        }
                    }
                    if(NTES.cookie.get(cookiename) != null && NTES.cookie.get(cookiename) != ""){
                        randowNumCookie = parseInt(NTES.cookie.get(cookiename));
